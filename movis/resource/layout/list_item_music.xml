<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:descendantFocusability="blocksDescendants"
    android:orientation="vertical">
    <RelativeLayout
        android:id="@id/music_item_above_layout"
        android:layout_width="wrap_content"
        android:layout_height="60.0dip">
        <com.videoai.mobile.component.imageview.DynamicLoadingImageView
            android:id="@id/music_item_waveform"
            android:layout_width="22.0dip"
            android:layout_height="22.0dip"
            android:layout_centerVertical="true"
            android:layout_marginStart="20.0dip"
            android:layout_marginLeft="20.0dip"
            android:visibility="gone" />
        <TextView
            android:id="@id/music_item_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="15.0dip"
            android:layout_marginLeft="15.0dip"
            android:textColor="@color/white_p30"
            android:textSize="12.0sp" />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toStartOf="@id/music_item_operation_layout"
            android:layout_toLeftOf="@id/music_item_operation_layout"
            android:layout_toEndOf="@id/music_item_time"
            android:layout_toRightOf="@id/music_item_time"
            android:orientation="vertical">
            <TextView
                android:id="@id/music_item_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16.0dip"
                android:layout_marginLeft="16.0dip"
                android:layout_marginEnd="10.0dip"
                android:layout_marginRight="10.0dip"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="aklsdjklasjdklasjd"
                android:textColor="@color/white_p90"
                android:textDirection="locale"
                android:textSize="16.0sp" />
            <TextView
                android:id="@id/music_item_author"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16.0dip"
                android:layout_marginLeft="16.0dip"
                android:layout_marginTop="2.0dip"
                android:layout_marginEnd="10.0dip"
                android:layout_marginRight="10.0dip"
                android:ellipsize="end"
                android:maxLines="1"
                android:text="aklsdjklasjdklasjd"
                android:textColor="@color/white_p40"
                android:textDirection="locale"
                android:textSize="12.0sp"
                android:visibility="gone" />
        </LinearLayout>
        <FrameLayout
            android:id="@id/music_item_operation_layout"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true">
            <com.videoai.aivpcore.ui.view.ProgressWheel xmlns:ProgressWheel="http://schemas.android.com/apk/res-auto"
                android:id="@id/music_item_progress"
                android:layout_width="35.0dip"
                android:layout_height="35.0dip"
                android:layout_gravity="end|center"
                android:layout_marginEnd="11.0dip"
                android:layout_marginRight="11.0dip"
                android:visibility="gone"
                ProgressWheel:pwBarColor="@color/default_app_color"
                ProgressWheel:pwBarWidth="2.0dip"
                ProgressWheel:pwCircleColor="@color/black"
                ProgressWheel:pwContourColor="@color/default_app_color"
                ProgressWheel:pwRimColor="@color/default_app_color"
                ProgressWheel:pwRimWidth="2.0px" />
            <ImageView
                android:id="@id/music_item_download"
                android:layout_width="50.0dip"
                android:layout_height="match_parent"
                android:layout_gravity="end|center"
                android:layout_marginEnd="3.5dip"
                android:layout_marginRight="3.5dip"
                android:scaleType="center"
                android:src="@drawable/xiaoying_music_item_download"
                android:visibility="gone" />
            <TextView
                android:id="@id/music_item_use"
                android:layout_width="wrap_content"
                android:layout_height="26.0dip"
                android:layout_gravity="end|center"
                android:layout_marginEnd="15.0dip"
                android:layout_marginRight="15.0dip"
                android:background="@drawable/xiaoying_music_item_add"
                android:gravity="center"
                android:minWidth="36.0dip"
                android:paddingStart="12.0dip"
                android:paddingLeft="12.0dip"
                android:paddingEnd="12.0dip"
                android:paddingRight="12.0dip"
                android:text="@string/xiaoying_str_com_add"
                android:textColor="@color/default_app_color"
                android:visibility="gone" />
            <ImageView
                android:id="@id/music_item_check_box"
                android:layout_width="50.0dip"
                android:layout_height="50.0dip"
                android:layout_gravity="end|center"
                android:scaleType="center"
                android:src="@drawable/xiaoying_music_item_check_selected"
                android:visibility="gone" />
            <ImageView
                android:id="@id/music_item_move"
                android:layout_width="50.0dip"
                android:layout_height="50.0dip"
                android:layout_gravity="end|center"
                android:scaleType="center"
                android:src="@drawable/xiaoying_music_item_move"
                android:visibility="gone" />
        </FrameLayout>
        <View
            android:id="@id/list_item_music_divider"
            android:layout_width="wrap_content"
            android:layout_height="1.0px"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="16.0dip"
            android:layout_marginLeft="16.0dip"
            android:layout_marginEnd="10.0dip"
            android:layout_marginRight="10.0dip"
            android:background="@color/white_p10"
            android:visibility="gone" />
    </RelativeLayout>
    <ViewStub
        android:id="@id/music_item_player"
        android:layout_width="match_parent"
        android:layout_height="100.0dip"
        android:layout="@layout/list_item_music_effect_player" />
</LinearLayout>
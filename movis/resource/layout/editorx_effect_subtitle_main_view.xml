<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@id/ll_seek_bar"
        android:layout_width="@dimen/px32"
        android:layout_height="0.0dip"
        android:layout_marginTop="@dimen/px48"
        android:layout_marginEnd="@dimen/px16"
        android:layout_marginBottom="@dimen/px12"
        android:background="@drawable/editorx_effect_text_size_layout_bg_shape"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@id/layout_edit"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintTop_toTopOf="parrent">
        <com.videoai.aivpcore.editorx.widget.seekbar.VerticalSeekBarWrapper
            android:layout_width="match_parent"
            android:layout_height="0.0dip"
            android:layout_weight="1.0">
            <com.videoai.aivpcore.editorx.widget.seekbar.FontSizeSeekBar
                android:id="@id/seek_font_size"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:maxHeight="2.0dip"
                app:fsb_min_progress="1"
                app:fsb_rotation="k"
                app:fsb_text_color="@color/white" />
        </com.videoai.aivpcore.editorx.widget.seekbar.VerticalSeekBarWrapper>
        <TextView
            android:id="@id/tv_size_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px2"
            android:layout_marginBottom="@dimen/px12"
            android:gravity="center_horizontal"
            android:text="@string/xiaoying_str_edit_effect_suntitle_font_size"
            android:textColor="@color/white"
            android:textSize="@dimen/sp12"
            android:textStyle="bold" />
    </LinearLayout>
    <LinearLayout
        android:id="@id/layout_reset"
        android:layout_width="wrap_content"
        android:layout_height="32.0dip"
        android:layout_marginEnd="24.0dip"
        android:layout_marginBottom="12.0dip"
        android:background="@drawable/editorx_text_all_text_shape"
        android:orientation="horizontal"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@id/layout_edit"
        app:layout_constraintEnd_toEndOf="parrent">
        <ImageView
            android:id="@id/text_reset_icon"
            android:layout_width="@dimen/px16"
            android:layout_height="@dimen/px16"
            android:scaleType="centerInside"
            android:src="@drawable/editorx_ico_effect_reset" />
        <TextView
            android:id="@id/text_reset_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6.0dip"
            android:text="@string/xiaoying_str_edit_reset_text"
            android:textColor="@color/white"
            android:textSize="@dimen/sp12" />
    </LinearLayout>
    <LinearLayout
        android:id="@id/layoutApply"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/px32"
        android:layout_marginStart="@dimen/px16"
        android:layout_marginBottom="@dimen/px12"
        android:background="@drawable/app_res_sample_5"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingStart="@dimen/px12"
        android:paddingEnd="@dimen/px12"
        app:layout_constraintBottom_toTopOf="@id/layout_edit"
        app:layout_constraintStart_toStartOf="parrent">
        <ImageView
            android:id="@id/ivApplySubtitle"
            android:layout_width="@dimen/px16"
            android:layout_height="@dimen/px16"
            android:src="@drawable/editorx_ico_apply_subtitle_unchoose" />
        <TextView
            android:id="@id/tvApplySubtitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/px6"
            android:gravity="center"
            android:text="@string/xiaoying_str_editor_all_text_text"
            android:textColor="@color/white"
            android:textSize="@dimen/sp12" />
    </LinearLayout>
    <com.videoai.aivpcore.editorx.widget.trimBar.TrimBarView
        android:id="@id/trim_bar_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px32"
        android:layout_marginBottom="@dimen/px68"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parrent" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@id/layout_edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/app_dcolor_background"
        android:clickable="true"
        android:focusable="true"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parrent">
        <com.videoai.aivpcore.editorx.board.effect.subtitle2.SubtitleKeyboardView
            android:id="@id/edit_view"
            android:layout_width="0.0dip"
            android:layout_height="@dimen/px42"
            app:layout_constraintEnd_toStartOf="@id/ivFinish"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toTopOf="parrent" />
        <View
            android:id="@id/edit_keyboard_space"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px214"
            app:layout_constraintBottom_toTopOf="@id/layout_tab"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/edit_view" />
        <ImageView
            android:id="@id/ivFinish"
            android:layout_width="@dimen/px42"
            android:layout_height="@dimen/px42"
            android:background="@drawable/btn_dcolor_ripple"
            android:scaleType="centerInside"
            android:src="@drawable/gallery_board_next_btn_selector"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintTop_toTopOf="parrent" />
        <com.videoai.aivpcore.editorx.board.effect.subtitle2.SubtitlePresetsView
            android:id="@id/preset_view"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px214"
            android:background="@color/app_dcolor_background"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@id/layout_tab"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/edit_view" />
        <com.videoai.aivpcore.editorx.board.effect.subtitle2.SubtitleCustomizeView
            android:id="@id/customize_view"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px214"
            android:background="@color/app_dcolor_background"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@id/layout_tab"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/edit_view" />
        <com.videoai.aivpcore.editorx.board.effect.subtitle2.SubtitleAnimationView
            android:id="@id/animation_view"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px214"
            android:background="@color/app_dcolor_background"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@id/layout_tab"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/edit_view" />
        <LinearLayout
            android:id="@id/layout_tab"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px56"
            android:background="@color/app_dcolor_bg_1"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parrent"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent">
            <com.videoai.aivpcore.editorx.board.effect.subtitle2.widget.TabFillView
                android:id="@id/tab_keyboard"
                android:layout_width="0.0dip"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent"
                app:tfv_tab_icon="@drawable/editorx_text_tab_keyboard_drawable"
                app:tfv_tab_title="@string/xiaoying_str_editor_text_keyboard_title"
                app:tfv_tab_title_color="@drawable/editorx_text_tab_text_color" />
            <com.videoai.aivpcore.editorx.board.effect.subtitle2.widget.TabFillView
                android:id="@id/tab_style"
                android:layout_width="0.0dip"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintStart_toEndOf="@id/tab_keyboard"
                app:layout_constraintTop_toTopOf="parrent"
                app:tfv_tab_icon="@drawable/editorx_text_tab_style_drawable"
                app:tfv_tab_title="@string/xiaoying_str_ve_undo_effect_style"
                app:tfv_tab_title_color="@drawable/editorx_text_tab_text_color" />
            <com.videoai.aivpcore.editorx.board.effect.subtitle2.widget.TabFillView
                android:id="@id/tab_font"
                android:layout_width="0.0dip"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintStart_toEndOf="@id/tab_effect"
                app:layout_constraintTop_toTopOf="parrent"
                app:tfv_tab_icon="@drawable/editorx_text_tab_font_drawable"
                app:tfv_tab_title="@string/xiaoying_str_ve_font_title"
                app:tfv_tab_title_color="@drawable/editorx_text_tab_text_color" />
            <com.videoai.aivpcore.editorx.board.effect.subtitle2.widget.TabFillView
                android:id="@id/tab_animation"
                android:layout_width="0.0dip"
                android:layout_height="match_parent"
                android:layout_weight="1.0"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintStart_toEndOf="@id/tab_effect"
                app:layout_constraintTop_toTopOf="parrent"
                app:tfv_tab_icon="@drawable/editorx_text_tab_animation_drawable"
                app:tfv_tab_title="@string/xiaoying_str_editor_text_animation_title"
                app:tfv_tab_title_color="@drawable/editorx_text_tab_text_color" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <include
        android:id="@id/layout_second"
        layout="@layout/editorx_effect_layout_second"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px56"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent" />
    <Button
        android:id="@id/btn_add_text"
        android:layout_width="104.0dip"
        android:layout_height="32.0dip"
        android:layout_above="@id/layout_second"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="12.0dip"
        android:background="@drawable/editorx_effect_subtitle_add_text"
        android:text="@string/xiaoying_str_editor_text_tip_add_character"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/sp12"
        android:textStyle="bold"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@id/layout_second"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent" />
    <com.videoai.aivpcore.editorx.widget.seekbar.CircleShadowView
        android:id="@id/subtitle_top_circle_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:csv_bg_color="@color/color_121212"
        app:csv_bg_diam="44.0dip"
        app:csv_border_color="@color/white"
        app:csv_border_width="3.0dip"
        app:csv_text="0"
        app:csv_text_color="@color/white"
        app:csv_text_size="18.0sp" />
</merge>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_dcolor_background"
    android:clipChildren="false"
    android:orientation="vertical">
    <LinearLayout
        android:id="@id/ll_content_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black"
        android:gravity="center"
        android:orientation="vertical">
        <include
            android:id="@id/title_bar"
            layout="@layout/xiaoying_music_extract_direct_title_bar_layout" />
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/extract_music_icon"
            android:layout_width="@dimen/px64"
            android:layout_height="@dimen/px64"
            android:layout_marginTop="@dimen/px42"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:src="@drawable/ic_extract_music_play" />
        <LinearLayout
            android:id="@id/layout_seek_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40.0dip"
            android:layout_marginBottom="20.0dip"
            android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@id/play_pause_btn"
                android:layout_width="@dimen/px24"
                android:layout_height="@dimen/px24"
                android:layout_marginStart="@dimen/px16"
                android:layout_marginEnd="@dimen/px2"
                android:src="@drawable/xiaoying_music_extract_direct_selector_video_play_pause"
                android:tint="@color/white" />
            <SeekBar
                android:id="@id/seekbar_simple_music"
                android:layout_width="0.0dip"
                android:layout_height="@dimen/px28"
                android:layout_weight="1.0"
                android:focusable="true"
                android:max="100"
                android:maxHeight="1.5dip"
                android:progress="0"
                android:progressDrawable="@drawable/xiaoying_music_extarct_direct_layer_preview_seekbar_progress"
                android:splitTrack="false"
                android:thumb="@drawable/xiaoying_music_extract_selector_video_seekbar"
                android:visibility="visible" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@id/play_music_duration"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/px8"
                android:layout_marginEnd="@dimen/px16"
                android:textColor="@color/white"
                android:textSize="@dimen/sp14"
                android:visibility="gone" />
        </LinearLayout>
    </LinearLayout>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@id/tv_name_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/ll_content_top"
        android:layout_marginStart="@dimen/px16"
        android:layout_marginTop="@dimen/px20"
        android:text="@string/xiaoying_str_community_account_info_title_name"
        android:textColor="@color/white"
        android:textSize="@dimen/sp14" />
    <LinearLayout
        android:id="@id/layout_rename"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_name_title"
        android:orientation="horizontal">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@id/layout_rename2"
            android:layout_width="0.0dip"
            android:layout_height="38.0dip"
            android:layout_marginStart="12.0dip"
            android:layout_marginEnd="12.0dip"
            android:layout_weight="1.0">
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@id/file_name_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="middle"
                android:fontFamily="@font/poppins_semibold"
                android:singleLine="true"
                android:textColor="@color/white"
                android:textSize="@dimen/sp12"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
            <androidx.appcompat.widget.AppCompatEditText
                android:id="@id/file_name_edittext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:imeOptions="actionDone"
                android:maxLength="50"
                android:singleLine="true"
                android:textColor="@color/white"
                android:textCursorDrawable="@color/colorPrimary"
                android:textSize="@dimen/sp12"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
            <View
                android:id="@id/edittext_line"
                android:layout_width="match_parent"
                android:layout_height="1.0dip"
                android:background="@color/color_eeeeee"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintLeft_toLeftOf="parrent"
                app:layout_constraintRight_toRightOf="parrent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@id/rename_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="@dimen/px16"
            android:background="@drawable/slt_item_tag"
            android:fontFamily="@font/poppins_semibold"
            android:paddingStart="@dimen/px12"
            android:paddingTop="8.0dip"
            android:paddingEnd="@dimen/px12"
            android:paddingBottom="8.0dip"
            android:text="@string/xiaoying_music_extract_rename_text"
            android:textColor="@color/text_selector_tab"
            android:textSize="@dimen/sp14" />
    </LinearLayout>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@id/save_success_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/layout_rename"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40.0dip"
        android:text="@string/xiaoying_str_com_clip_save_success_tip"
        android:textColor="@color/default_app_color"
        android:textSize="@dimen/sp14"
        android:visibility="invisible" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@id/save_path_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/save_success_label"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8.0dip"
        android:textColor="@color/default_app_color"
        android:textSize="12.0sp"
        android:visibility="invisible" />
    <RelativeLayout
        android:id="@id/layout_save_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@id/discard_and_quit_btn"
        android:layout_marginStart="@dimen/px16"
        android:layout_marginTop="5.0dip"
        android:layout_marginEnd="@dimen/px16">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/iv_free_of_time_limit"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/px16"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="-8.0dip"
            android:layout_marginEnd="8.0dip"
            android:visibility="gone" />
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@id/save_btn"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px48"
            android:layout_centerInParent="true"
            android:background="@drawable/app_default_button"
            android:fontFamily="@font/poppins_semibold"
            android:gravity="center"
            android:text="@string/xiaoying_music_extract_direct_save_local_btn_text"
            android:textColor="@color/white"
            android:textSize="@dimen/sp14" />
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/iv_vip_func"
            android:layout_width="@dimen/px16"
            android:layout_height="@dimen/px16"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/px42"
            android:visibility="gone" />
    </RelativeLayout>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@id/discard_and_quit_btn"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px48"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/px16"
        android:layout_marginTop="@dimen/px12"
        android:layout_marginEnd="@dimen/px16"
        android:layout_marginBottom="@dimen/px20"
        android:background="@drawable/app_res_sample_2"
        android:gravity="center"
        android:text="@string/xiaoying_music_extract_direct_discard_and_quit"
        android:textColor="@color/color_primary"
        android:textSize="@dimen/sp14" />
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <View
        android:id="@+id/pmm_top_view"
        android:layout_width="11.0dip"
        android:layout_height="11.0dip"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.mobile.component.imageview.DynamicLoadingImageView
        android:id="@+id/pmm_item_icon"
        android:layout_width="60.0dip"
        android:layout_height="60.0dip"
        android:layout_marginStart="4.0dip"
        android:layout_marginTop="3.0dip"
        android:layout_marginEnd="4.0dip"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toEndOf="@id/pmm_top_view"
        app:layout_constraintTop_toTopOf="parrent"
        app:roundBottomLeft="true"
        app:roundBottomRight="true"
        app:roundTopLeft="true"
        app:roundTopRight="true" />
    <View
        android:id="@+id/pmm_stoken"
        android:layout_width="66.0dip"
        android:layout_height="66.0dip"
        android:background="@drawable/editorx_shape_effect_pip_mix_model_stoken"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/pmm_item_icon"
        app:layout_constraintEnd_toEndOf="@id/pmm_item_icon"
        app:layout_constraintStart_toStartOf="@id/pmm_item_icon"
        app:layout_constraintTop_toTopOf="@id/pmm_item_icon" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/pmm_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8.0dip"
        android:gravity="center"
        android:includeFontPadding="false"
        android:lines="1"
        android:textColor="@color/white"
        app:autoSizeStepGranularity="1.0sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="@id/pmm_item_icon"
        app:layout_constraintStart_toStartOf="@id/pmm_item_icon"
        app:layout_constraintTop_toBottomOf="@id/pmm_item_icon" />
</androidx.constraintlayout.widget.ConstraintLayout>
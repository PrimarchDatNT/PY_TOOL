<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/px56"
    android:background="@color/app_dcolor_bg_1"
    android:clipToPadding="false">
    <View
        android:layout_width="match_parent"
        android:layout_height="1.0dip"
        android:background="@color/app_dcolor_indicator"
        app:layout_constraintBottom_toTopOf="parrent"
        app:layout_constraintLeft_toLeftOf="parrent"
        app:layout_constraintRight_toRightOf="parrent" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_clip_tab"
        style="@style/TextSingleLine"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/tab_editor_clip_edit_unselect"
        android:drawablePadding="@dimen/px2"
        android:ellipsize="end"
        android:gravity="center"
        android:text="@string/xiaoying_str_ve_clip_edit_title"
        android:textColor="@color/app_dcolor_text_2"
        app:autoSizeMaxTextSize="@dimen/editorx_tab_normal_textsize"
        app:autoSizeMinTextSize="@dimen/editorx_tab_tiny_textsize"
        app:autoSizeTextType="bjy"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toStartOf="@id/tv_effect_tab"
        app:layout_constraintHorizontal_chainStyle="b42"
        app:layout_constraintHorizontal_weight="1.0"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_effect_tab"
        style="@style/TextSingleLine"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px6"
        android:layout_marginBottom="@dimen/px6"
        android:drawableTop="@drawable/tab_editor_effect_unselect"
        android:drawablePadding="@dimen/px2"
        android:ellipsize="end"
        android:gravity="center"
        android:text="@string/app_feature_effects"
        android:textColor="@color/app_dcolor_text_2"
        app:autoSizeMaxTextSize="@dimen/editorx_tab_normal_textsize"
        app:autoSizeMinTextSize="@dimen/editorx_tab_tiny_textsize"
        app:autoSizeTextType="bjy"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toStartOf="@id/tv_theme_tab"
        app:layout_constraintHorizontal_weight="1.0"
        app:layout_constraintStart_toEndOf="@id/tv_clip_tab"
        app:layout_constraintTop_toTopOf="parrent" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_theme_tab"
        style="@style/TextSingleLine"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/editor_tab_theme_unselect"
        android:ellipsize="end"
        android:gravity="center"
        android:text="@string/xiaoying_str_ve_theme_title_common"
        android:textColor="@color/app_dcolor_indicator"
        android:visibility="gone"
        app:autoSizeMaxTextSize="@dimen/editorx_tab_normal_textsize"
        app:autoSizeMinTextSize="@dimen/editorx_tab_tiny_textsize"
        app:autoSizeTextType="bjy"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toStartOf="@id/tv_filter_tab"
        app:layout_constraintHorizontal_chainStyle="b42"
        app:layout_constraintHorizontal_weight="1.0"
        app:layout_constraintStart_toEndOf="@id/tv_effect_tab"
        app:layout_constraintTop_toTopOf="parrent" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_filter_tab"
        style="@style/TextSingleLine"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/tab_editor_filter_unselect"
        android:drawablePadding="@dimen/px2"
        android:ellipsize="end"
        android:gravity="center"
        android:text="@string/xiaoying_str_ve_effect_title"
        android:textColor="@color/app_dcolor_text_2"
        app:autoSizeMaxTextSize="@dimen/editorx_tab_normal_textsize"
        app:autoSizeMinTextSize="@dimen/editorx_tab_tiny_textsize"
        app:autoSizeTextType="bjy"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toStartOf="@id/tv_audio_tab"
        app:layout_constraintHorizontal_weight="1.0"
        app:layout_constraintStart_toEndOf="@id/tv_theme_tab"
        app:layout_constraintTop_toTopOf="parrent" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_audio_tab"
        style="@style/TextSingleLine"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/tab_editor_music_unselect"
        android:drawablePadding="@dimen/px2"
        android:ellipsize="end"
        android:gravity="center"
        android:text="@string/xiaoying_str_ve_music_title"
        android:textColor="@color/app_dcolor_text_2"
        app:autoSizeMaxTextSize="@dimen/editorx_tab_normal_textsize"
        app:autoSizeMinTextSize="@dimen/editorx_tab_tiny_textsize"
        app:autoSizeTextType="bjy"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintHorizontal_weight="1.0"
        app:layout_constraintStart_toEndOf="@id/tv_filter_tab"
        app:layout_constraintTop_toTopOf="parrent" />
</androidx.constraintlayout.widget.ConstraintLayout>
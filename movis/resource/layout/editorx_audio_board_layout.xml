<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <FrameLayout
        android:id="@id/multi_level_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.videoai.aivpcore.editorx.board.audio.base.RootOperateView
        android:id="@id/root_operate_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/px56" />
    <ImageView
        android:id="@id/imageview"
        android:layout_width="50.0dip"
        android:layout_height="50.0dip"
        android:layout_gravity="bottom|center"
        android:layout_marginBottom="100.0dip"
        android:scaleType="fitXY" />
    <com.videoai.aivpcore.editorx.widget.GuideView
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</RelativeLayout>
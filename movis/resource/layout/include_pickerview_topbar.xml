<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/az7"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/vz">
    <Button
        android:id="@id/jd"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:background="@android:color/transparent"
        android:gravity="center|left"
        android:paddingLeft="@dimen/sz"
        android:text="Cancel"
        android:textColor="@drawable/apm"
        android:textSize="@dimen/sx" />
    <TextView
        android:id="@id/bcb"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@id/k6"
        android:layout_toRightOf="@id/jd"
        android:gravity="center"
        android:textColor="@color/w2"
        android:textSize="@dimen/t0" />
    <Button
        android:id="@id/k6"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:background="@android:color/transparent"
        android:gravity="center|right"
        android:paddingRight="@dimen/sz"
        android:text="Submit"
        android:textColor="@drawable/apm"
        android:textSize="@dimen/sx" />
</RelativeLayout>
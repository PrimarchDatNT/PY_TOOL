<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:id="@id/dialog_bg"
        android:layout_width="match_parent"
        android:layout_height="90.0dip"
        android:layout_marginLeft="15.0dip"
        android:layout_marginTop="58.0dip"
        android:layout_marginRight="15.0dip"
        android:background="@drawable/editorx_shape_audio_volume_dialog_bg"
        app:layout_constraintBottom_toBottomOf="@id/popSeekBar"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.aivpcore.editorx.widget.seekbar.PopSeekBar
        android:id="@id/popSeekBar"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/dialog_bg"
        app:layout_constraintStart_toStartOf="@id/dialog_bg"
        app:layout_constraintTop_toTopOf="parrent"
        app:psb_bg_color="@color/default_background_pb_progress_color"
        app:psb_max_progress="90"
        app:psb_seek_bar_color="@color/default_app_color"
        app:psb_seek_bar_left_padding="24.0dip"
        app:psb_seek_bar_progress_drawable="@drawable/psb_progress_drawable"
        app:psb_seek_bar_right_padding="24.0dip"
        app:psb_seek_bar_thumb="@drawable/psb_selector_seek_bar_thumb"
        app:psb_text_bg_space="24.0dip"
        app:psb_top_text_color="@color/color_333333"
        app:psb_top_text_diam="44.0dip"
        app:psb_top_text_size="@dimen/sp16" />
    <TextView
        android:id="@id/tvTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12.0dip"
        android:text="@string/xiaoying_str_edit_clip_back_angle"
        android:textColor="@color/white"
        android:textSize="@dimen/sp12"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/dialog_bg"
        app:layout_constraintStart_toStartOf="@id/dialog_bg"
        app:layout_constraintTop_toTopOf="@id/dialog_bg" />
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/layout_guide_dialog"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@id/v_dialog"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.65"
        android:background="@color/black_p10"
        android:clickable="true"
        android:focusable="true" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@id/cl_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40.0dip"
        android:layout_marginRight="40.0dip"
        android:background="@drawable/editorx_shape_rect_bg_ffffff_r_16"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintTop_toTopOf="parrent">
        <TextView
            android:id="@id/tv_title1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px12"
            android:text="@string/xiaoying_str_editor_guide_antic"
            android:textColor="@color/white"
            android:textSize="@dimen/sp16"
            app:layout_constraintBottom_toTopOf="@id/tv_title2"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toTopOf="parrent" />
        <TextView
            android:id="@id/tv_title2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/px16"
            android:layout_marginTop="@dimen/px8"
            android:layout_marginEnd="@dimen/px16"
            android:gravity="center"
            android:text="@string/xiaoying_str_editor_guide_pinch_title"
            android:textColor="@color/white"
            android:textSize="@dimen/sp12"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/tv_title1" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@id/cl_pic"
            android:layout_width="match_parent"
            android:layout_height="0.0dip"
            app:layout_constraintDimensionRatio="1.134615385"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/tv_title2">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@id/iv_guide"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/editorx_guide_1" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@id/tv_tip1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/px16"
                android:layout_marginEnd="@dimen/px16"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="@dimen/sp10"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@id/tv_tip2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/px16"
                android:layout_marginEnd="@dimen/px16"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="@dimen/sp10"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintStart_toStartOf="parrent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@id/tv_got"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px42"
            android:layout_marginStart="@dimen/px16"
            android:layout_marginTop="@dimen/px12"
            android:layout_marginEnd="@dimen/px16"
            android:layout_marginBottom="@dimen/px12"
            android:background="@drawable/app_res_sample_2"
            android:gravity="center"
            android:text="@string/xiaoying_str_com_msg_got_it"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/sp14"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parrent"
            app:layout_constraintLeft_toLeftOf="parrent"
            app:layout_constraintRight_toRightOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/cl_pic" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="53.0dip"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/cl_pic">
            <View
                android:id="@id/v_center"
                android:layout_width="0.5dip"
                android:layout_height="match_parent"
                android:background="#fff2f2f2"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
            <TextView
                android:id="@id/tv_detail"
                android:layout_width="0.0dip"
                android:layout_height="0.0dip"
                android:gravity="center"
                android:text="@string/xiaoying_str_go_detail_title"
                android:textColor="@color/color_ff6333"
                android:textSize="16.0sp"
                android:textStyle="bold"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintEnd_toStartOf="@id/v_center"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
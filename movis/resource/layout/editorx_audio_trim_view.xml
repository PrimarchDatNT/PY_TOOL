<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/audio_trim_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_p10">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px270"
        android:background="@color/app_dcolor_background"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent">
        <LinearLayout
            android:id="@+id/audio_trim_title_layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px48"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingLeft="77.0dip"
            android:paddingRight="77.0dip"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toTopOf="parrent">
            <com.videoai.mobile.component.imageview.DynamicLoadingImageView
                android:id="@+id/audio_trim_music_icon"
                android:layout_width="20.0dip"
                android:layout_height="20.0dip"
                app:actualImageScaleType="o1"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintLeft_toLeftOf="parrent"
                app:layout_constraintRight_toRightOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent"
                app:placeholderImageScaleType="o1" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/audio_trim_music_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8.0dip"
                android:ellipsize="end"
                android:lines="1"
                android:text="@string/xiaoying_str_ve_basic_trim_title"
                android:textColor="@color/white"
                android:textSize="@dimen/sp14"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
        </LinearLayout>
        <View
            android:id="@+id/audio_trim_top_line"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px1"
            android:layout_marginTop="@dimen/px48"
            android:background="@color/app_dcolor_indicator"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toTopOf="parrent" />
        <com.videoai.aivpcore.timeline.fixed.music.MusicTrimTimeline
            android:id="@+id/audio_trim_time_line"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/px60"
            android:layout_marginBottom="@dimen/px56"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/audio_trim_top_line" />
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginStart="@dimen/px56"
            android:layout_marginTop="184.0dip"
            android:text="@string/xiaoying_str_edit_audio_vloume_text"
            android:textColor="@color/white"
            android:textSize="@dimen/sp12"
            android:visibility="gone" />
        <com.videoai.aivpcore.editorx.widget.seekbar.PopSeekBar
            android:id="@+id/audio_trim_ver_seek_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="96.0dip"
            android:visibility="gone"
            app:psb_bg_color="@color/color_212121"
            app:psb_bg_max_progress_color="@color/color_525252"
            app:psb_max_progress="200"
            app:psb_seek_bar_color="@color/color_525252"
            app:psb_seek_bar_left_padding="73.0dip"
            app:psb_seek_bar_progress_drawable="@drawable/psb_music_cut_progress_drawable"
            app:psb_seek_bar_right_padding="32.0dip"
            app:psb_seek_bar_thumb="@drawable/msb_layer_pop_seek_bar_thumb_gray_52"
            app:psb_text_bg_space="24.0dip"
            app:psb_top_text_color="@color/color_333333"
            app:psb_top_text_diam="44.0dip"
            app:psb_top_text_size="18.0sp" />
        <com.videoai.aivpcore.editorx.board.clip.widget.TextActionBottomBar
            android:id="@+id/audio_trim_bottom_label_text"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px56"
            android:layout_gravity="bottom" />
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
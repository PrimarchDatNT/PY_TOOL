<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="76.0dip"
    android:background="@color/transparent">
    <com.videoai.aivpcore.editor.videotrim.ui.VePIPGallery
        android:id="@id/gallery_timeline"
        android:layout_width="match_parent"
        android:layout_height="32.0dip"
        android:layout_marginTop="5.0dip"
        android:background="@color/color_808080"
        android:fadingEdge="none"
        android:gravity="center_vertical"
        android:spacing="0.0dip"
        android:unselectedAlpha="1.0" />
    <com.videoai.aivpcore.editor.videotrim.ui.VePIPGallery
        android:id="@id/xiaoying_ve_pip_gallery_timeline"
        android:layout_width="match_parent"
        android:layout_height="32.0dip"
        android:layout_below="@id/gallery_timeline"
        android:layout_marginTop="2.0px"
        android:background="@color/color_808080"
        android:fadingEdge="none"
        android:gravity="center_vertical"
        android:spacing="0.0dip"
        android:unselectedAlpha="1.0" />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2.0px"
        android:layout_below="@id/gallery_timeline"
        android:background="@color/color_808080" />
    <com.videoai.aivpcore.editor.pip.TrimMaskView xmlns:trimmaskview="http://schemas.android.com/apk/res-auto"
        android:id="@id/xiaoying_ve_trimmaskview"
        android:layout_width="match_parent"
        android:layout_height="76.0dip"
        android:visibility="visible"
        trimmaskview:curneedle="@drawable/xiaoying_ve_trim_play_needle"
        trimmaskview:leftfakehandle="@drawable/xiaoying_ve_trim_bar_fake_double_left"
        trimmaskview:lefthandle="@drawable/v4_xiaoying_ve_trim_double_leftbar_selector"
        trimmaskview:rightfakehandle="@drawable/xiaoying_ve_trim_bar_fake_double_left"
        trimmaskview:righthandle="@drawable/v4_xiaoying_ve_trim_double_leftbar_selector" />
</RelativeLayout>
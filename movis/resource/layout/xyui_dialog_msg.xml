<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32.0dip"
        android:layout_marginTop="32.0dip"
        android:layout_marginRight="32.0dip"
        android:gravity="center"
        android:text=""
        android:textColor="@color/color_333333"
        android:textSize="18.0sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <TextView
        android:id="@+id/tvDesc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32.0dip"
        android:layout_marginTop="12.0dip"
        android:layout_marginRight="32.0dip"
        android:gravity="center"
        android:textColor="@color/color_cccccc"
        android:textSize="14.0sp"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/tvTitle" />
    <TextView
        android:id="@+id/tvExtra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32.0dip"
        android:layout_marginTop="20.0dip"
        android:layout_marginRight="32.0dip"
        android:gravity="center"
        android:textColor="@color/color_ff6333"
        android:textSize="14.0sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/tvDesc" />
    <TextView
        android:id="@+id/btnMain"
        android:layout_width="0.0dip"
        android:layout_height="48.0dip"
        android:layout_marginLeft="32.0dip"
        android:layout_marginTop="28.0dip"
        android:layout_marginRight="32.0dip"
        android:background="@drawable/xyui_shape_dialog_positive_button"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="16.0sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/tvExtra" />
    <TextView
        android:id="@+id/btnCancel"
        android:layout_width="wrap_content"
        android:layout_height="24.0dip"
        android:layout_marginLeft="52.0dip"
        android:layout_marginTop="20.0dip"
        android:layout_marginRight="52.0dip"
        android:gravity="center"
        android:textColor="@color/color_ff6333"
        android:textSize="16.0sp"
        android:textStyle="bold"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@id/viewBottomGap"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/btnMain" />
    <com.videoai.aivpcore.xyui.RoundedTextView
        android:id="@+id/btnSub"
        android:layout_width="0.0dip"
        android:layout_height="48.0dip"
        android:layout_marginLeft="32.0dip"
        android:layout_marginTop="20.0dip"
        android:layout_marginRight="32.0dip"
        android:layout_marginBottom="12.0dip"
        android:gravity="center"
        android:textColor="@color/color_333333"
        android:textSize="16.0sp"
        android:textStyle="bold"
        android:visibility="gone"
        app:VS_CornerRadius="26.0dip"
        app:VS_SolidColor="@color/color_f2f2f2"
        app:layout_constraintBottom_toTopOf="@id/viewBottomGap"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/btnMain" />
    <View
        android:id="@+id/viewBottomGap"
        android:layout_width="0.0dip"
        android:layout_height="20.0dip"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent" />
</androidx.constraintlayout.widget.ConstraintLayout>
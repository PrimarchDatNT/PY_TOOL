<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/tt_bottom_bar"
    android:layout_width="match_parent"
    android:layout_height="44.5dip"
    android:layout_alignParentBottom="true"
    android:layout_centerVertical="true"
    android:background="#ffffffff"
    android:clickable="true"
    android:focusable="true"
    android:orientation="vertical">
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="#1f161823" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="44.0dip"
        android:orientation="horizontal">
        <ImageView
            android:id="@id/tt_bottom_bar_back"
            android:layout_width="40.0dip"
            android:layout_height="44.0dip"
            android:layout_marginStart="16.0dip"
            android:layout_marginLeft="16.0dip"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="14.75dip"
            android:paddingTop="12.5dip"
            android:paddingRight="14.75dip"
            android:paddingBottom="12.5dip"
            android:src="@drawable/tt_ad_arrow_backward" />
        <View
            android:layout_width="0.0dip"
            android:layout_height="0.0dip"
            android:layout_weight="1.0" />
        <ImageView
            android:id="@id/tt_bottom_bar_forward"
            android:layout_width="40.0dip"
            android:layout_height="44.0dip"
            android:layout_marginStart="8.0dip"
            android:layout_marginLeft="8.0dip"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="14.75dip"
            android:paddingTop="12.5dip"
            android:paddingRight="14.75dip"
            android:paddingBottom="12.5dip"
            android:src="@drawable/tt_ad_arrow_forward" />
        <View
            android:layout_width="0.0dip"
            android:layout_height="0.0dip"
            android:layout_weight="1.0" />
        <ImageView
            android:id="@id/tt_bottom_bar_refresh"
            android:layout_width="40.0dip"
            android:layout_height="44.0dip"
            android:layout_marginStart="8.0dip"
            android:layout_marginLeft="8.0dip"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10.0dip"
            android:paddingTop="12.0dip"
            android:paddingRight="10.0dip"
            android:paddingBottom="12.0dip"
            android:src="@drawable/tt_ad_refresh" />
        <View
            android:layout_width="0.0dip"
            android:layout_height="0.0dip"
            android:layout_weight="1.0" />
        <ImageView
            android:id="@id/tt_bottom_bar_go_to_browser"
            android:layout_width="40.0dip"
            android:layout_height="44.0dip"
            android:layout_marginEnd="16.0dip"
            android:layout_marginRight="16.0dip"
            android:paddingLeft="9.0dip"
            android:paddingTop="11.0dip"
            android:paddingRight="9.0dip"
            android:paddingBottom="11.0dip"
            android:src="@drawable/tt_ad_link" />
    </LinearLayout>
</LinearLayout>
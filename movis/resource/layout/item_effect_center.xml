<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/app_res_sample_3"
    android:orientation="vertical"
    android:tag="layout/item_effect_center_0">
    <com.videoai.aivpcore.unit.view.PressImageView
        android:id="@+id/iv_effect_banner"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px116"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop"
        app:shapeAppearanceOverlay="@style/RoundedImageView" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_effect_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:padding="@dimen/px8"
        android:text="@string/app_template_theme"
        android:textColor="@color/white"
        android:textSize="@dimen/sp14"
        android:textStyle="bold" />
</LinearLayout>
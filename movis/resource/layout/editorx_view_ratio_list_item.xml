<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="match_parent">
    <com.videoai.mobile.component.imageview.XYImageView
        android:id="@id/viewBg"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:background="@color/color_363636"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:roundedCornerRadius="4.0dip" />
    <TextView
        android:id="@id/tvRatio"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8.0dip"
        android:layout_marginRight="8.0dip"
        android:drawablePadding="4.0dip"
        android:gravity="center"
        android:textColor="@color/editorx_sel_ratio_text"
        android:textSize="@dimen/sp12"
        app:layout_constraintBottom_toBottomOf="@id/viewBg"
        app:layout_constraintEnd_toEndOf="@id/viewBg"
        app:layout_constraintStart_toStartOf="@id/viewBg"
        app:layout_constraintTop_toTopOf="@id/viewBg" />
    <com.videoai.aivpcore.xyui.RoundedRelativeLayout
        android:id="@id/ivFoucs"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:visibility="gone"
        app:VS_CornerRadius="@dimen/px6"
        app:VS_StrokeColor="@color/colorPrimary"
        app:VS_StrokeWidth="@dimen/px1"
        app:layout_constraintBottom_toBottomOf="@id/viewBg"
        app:layout_constraintEnd_toEndOf="@id/viewBg"
        app:layout_constraintStart_toStartOf="@id/viewBg"
        app:layout_constraintTop_toTopOf="@id/viewBg" />
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginStart="25.0dip"
        android:layout_marginLeft="25.0dip"
        android:layout_marginEnd="25.0dip"
        android:layout_marginRight="25.0dip">
        <com.videoai.aivpcore.ui.dialog.MDRootLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@id/md_root_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/v4_xiaoying_com_dialog_bg"
            android:orientation="vertical"
            app:md_reduce_padding_no_title_no_buttons="false">
            <include layout="@layout/xiaoying_com_dialog_stub_titleframe" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:orientation="vertical"
                android:paddingStart="@dimen/md_dialog_frame_margin"
                android:paddingLeft="@dimen/md_dialog_frame_margin"
                android:paddingTop="@dimen/md_content_padding_top"
                android:paddingEnd="@dimen/md_dialog_frame_margin"
                android:paddingRight="@dimen/md_dialog_frame_margin"
                android:paddingBottom="@dimen/md_content_padding_bottom">
                <include layout="@layout/xiaoying_com_dialog_stub_progress" />
            </LinearLayout>
            <include layout="@layout/xiaoying_com_dialog_stub_actionbuttons" />
        </com.videoai.aivpcore.ui.dialog.MDRootLayout>
    </RelativeLayout>
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_dcolor_background"
    android:gravity="start"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px42"
        android:gravity="center">
        <com.videoai.aivpcore.editorx.board.effect.subtitle2.widget.TabIndicatorView
            android:id="@+id/tv_typeface"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:tab_title="@string/xiaoying_str_ve_font_title" />
        <com.videoai.aivpcore.editorx.board.effect.subtitle2.widget.TabIndicatorView
            android:id="@+id/tv_text"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:tab_title="@string/xiaoying_effect_subtitle_text" />
        <com.videoai.aivpcore.editorx.board.effect.subtitle2.widget.TabIndicatorView
            android:id="@+id/tv_stroke"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:tab_title="@string/xiaoying_str_ve_stroke" />
        <com.videoai.aivpcore.editorx.board.effect.subtitle2.widget.TabIndicatorView
            android:id="@+id/tv_shadow"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:tab_title="@string/xiaoying_str_ve_video_param_shadow" />
    </LinearLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0.0dip"
        android:layout_weight="1.0">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="bottom">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_inner_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_color_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:gravity="center_vertical"
                        android:text="@string/xiaoying_str_editor_text_fill_text_text"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        android:textStyle="bold"
                        android:visibility="visible"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <TextView
                        android:id="@+id/tv_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/px12"
                        android:layout_marginEnd="@dimen/px20"
                        android:gravity="center_vertical"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <com.videoai.aivpcore.editorx.widget.seekbar.ColorBarBgView
                        android:id="@+id/colorbar_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/px12"
                        app:csb_bg_height="@dimen/px20"
                        app:csb_bg_left_padding="@dimen/px20"
                        app:csb_bg_right_padding="@dimen/px20"
                        app:csb_color_type="b83"
                        app:csb_thumb_border_color="@color/white"
                        app:csb_thumb_border_width="1.0dip"
                        app:csb_thumb_height="@dimen/px24"
                        app:csb_thumb_width="@dimen/px18"
                        app:layout_constraintLeft_toLeftOf="parrent"
                        app:layout_constraintRight_toRightOf="parrent"
                        app:layout_constraintTop_toBottomOf="@id/tv_color_title" />
                    <TextView
                        android:id="@+id/tv_alpha_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:gravity="center_vertical"
                        android:text="@string/xiaoying_str_edit_clip_alpha"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        android:textStyle="bold"
                        android:visibility="visible"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toBottomOf="@id/colorbar_text_color" />
                    <TextView
                        android:id="@+id/tv_alpha_progress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/px20"
                        android:textColor="@color/color_525252"
                        android:textSize="@dimen/sp12"
                        app:layout_constraintBottom_toBottomOf="@id/tv_alpha_title"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintTop_toTopOf="@id/tv_alpha_title" />
                    <com.videoai.aivpcore.editorx.widget.seekbar.VHSeekBar
                        android:id="@+id/seek_font_alpha"
                        android:layout_width="0.0dip"
                        android:layout_height="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:layout_marginEnd="8.0dip"
                        android:paddingStart="@dimen/px20"
                        android:paddingEnd="20.0dip"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toBottomOf="@id/tv_alpha_title"
                        app:vh_bg_color="@color/app_dcolor_indicator_2"
                        app:vh_bg_radius="@dimen/px20"
                        app:vh_max_progress="100"
                        app:vh_orientation="a30"
                        app:vh_point_color="@color/white"
                        app:vh_point_radius="@dimen/px16"
                        app:vh_progress="0"
                        app:vh_progress_color="@color/colorPrimary"
                        app:vh_progress_radius="20.0dip"
                        app:vh_top_bg_space="@dimen/px20" />
                    <TextView
                        android:id="@+id/tv_align_title"
                        android:layout_width="@dimen/px48"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/px20"
                        android:gravity="center_vertical"
                        android:text="@string/xiaoying_str_edit_effect_suntitle_font_align"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@id/iv_align_left"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="@id/iv_align_left" />
                    <ImageView
                        android:id="@+id/iv_align_left"
                        android:layout_width="@dimen/px48"
                        android:layout_height="@dimen/px24"
                        android:layout_marginStart="@dimen/px12"
                        android:layout_marginTop="@dimen/px12"
                        android:paddingStart="@dimen/px12"
                        android:paddingEnd="@dimen/px12"
                        android:scaleType="centerInside"
                        android:src="@drawable/editorx_icon_subtitle_font_align_left"
                        app:layout_constraintStart_toEndOf="@id/tv_align_title"
                        app:layout_constraintTop_toBottomOf="@id/seek_font_alpha" />
                    <ImageView
                        android:id="@+id/iv_align_center"
                        android:layout_width="@dimen/px48"
                        android:layout_height="@dimen/px24"
                        android:layout_marginStart="@dimen/px8"
                        android:paddingStart="@dimen/px12"
                        android:paddingEnd="@dimen/px12"
                        android:scaleType="centerInside"
                        android:src="@drawable/editorx_icon_subtitle_font_align_center"
                        app:layout_constraintBottom_toBottomOf="@id/iv_align_left"
                        app:layout_constraintStart_toEndOf="@id/iv_align_left"
                        app:layout_constraintTop_toTopOf="@id/iv_align_left" />
                    <ImageView
                        android:id="@+id/iv_align_right"
                        android:layout_width="@dimen/px48"
                        android:layout_height="@dimen/px24"
                        android:layout_marginStart="@dimen/px8"
                        android:paddingStart="@dimen/px12"
                        android:paddingEnd="@dimen/px12"
                        android:scaleType="centerInside"
                        android:src="@drawable/editorx_icon_suntitle_font_align_right"
                        app:layout_constraintBottom_toBottomOf="@id/iv_align_left"
                        app:layout_constraintStart_toEndOf="@id/iv_align_center"
                        app:layout_constraintTop_toTopOf="@id/iv_align_left" />
                    <ImageView
                        android:id="@+id/iv_align_top"
                        android:layout_width="@dimen/px48"
                        android:layout_height="@dimen/px24"
                        android:layout_marginStart="@dimen/px8"
                        android:paddingStart="@dimen/px12"
                        android:paddingEnd="@dimen/px12"
                        android:scaleType="centerInside"
                        android:src="@drawable/editorx_icon_suntitle_font_align_top"
                        android:visibility="visible"
                        app:layout_constraintBottom_toBottomOf="@id/iv_align_left"
                        app:layout_constraintStart_toEndOf="@id/iv_align_right"
                        app:layout_constraintTop_toTopOf="@id/iv_align_left" />
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_inner_stroke"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/stroke_color_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:gravity="center_vertical"
                        android:text="@string/xiaoying_str_editor_text_fill_text_text"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        android:textStyle="bold"
                        android:visibility="visible"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <TextView
                        android:id="@+id/stroke_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/px12"
                        android:layout_marginEnd="@dimen/px20"
                        android:gravity="center_vertical"
                        android:text="None"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <ImageView
                        android:id="@+id/stroke_color_none"
                        android:layout_width="@dimen/px16"
                        android:layout_height="@dimen/px20"
                        android:layout_marginStart="@dimen/px20"
                        android:src="@drawable/editorx_text_none_color_choose_icon_selector"
                        app:layout_constraintBottom_toBottomOf="@id/colorbar_stroke_color"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="@id/colorbar_stroke_color" />
                    <com.videoai.aivpcore.editorx.widget.seekbar.ColorBarBgView
                        android:id="@+id/colorbar_stroke_color"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/px12"
                        app:csb_bg_height="@dimen/px20"
                        app:csb_bg_left_padding="@dimen/px16"
                        app:csb_bg_right_padding="@dimen/px20"
                        app:csb_color_type="b83"
                        app:csb_thumb_border_color="@color/white"
                        app:csb_thumb_border_width="1.0dip"
                        app:csb_thumb_height="@dimen/px24"
                        app:csb_thumb_width="@dimen/px18"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toEndOf="@id/stroke_color_none"
                        app:layout_constraintTop_toBottomOf="@id/stroke_color_title" />
                    <TextView
                        android:id="@+id/tv_stroke_title"
                        android:layout_width="@dimen/px48"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:gravity="center_vertical"
                        android:text="@string/xiaoying_str_editor_text_tip_width"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toBottomOf="@id/colorbar_stroke_color" />
                    <TextView
                        android:id="@+id/tv_stroke_progress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/px20"
                        android:textColor="@color/color_525252"
                        android:textSize="@dimen/sp12"
                        app:layout_constraintBottom_toBottomOf="@id/tv_stroke_title"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintTop_toTopOf="@id/tv_stroke_title" />
                    <com.videoai.aivpcore.editorx.widget.seekbar.VHSeekBar
                        android:id="@+id/seek_font_stroke"
                        android:layout_width="0.0dip"
                        android:layout_height="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:paddingStart="@dimen/px20"
                        android:paddingEnd="20.0dip"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toBottomOf="@id/tv_stroke_title"
                        app:vh_bg_color="@color/app_dcolor_indicator_2"
                        app:vh_bg_radius="@dimen/px20"
                        app:vh_max_progress="100"
                        app:vh_orientation="a30"
                        app:vh_point_color="@color/white"
                        app:vh_point_radius="@dimen/px16"
                        app:vh_progress="0"
                        app:vh_progress_color="@color/colorPrimary"
                        app:vh_progress_radius="20.0dip"
                        app:vh_top_bg_space="@dimen/px20" />
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_inner_shadow"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/shadow_color_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:gravity="center_vertical"
                        android:text="@string/xiaoying_str_editor_text_fill_text_text"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        android:textStyle="bold"
                        android:visibility="visible"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <TextView
                        android:id="@+id/shadow_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/px12"
                        android:layout_marginEnd="@dimen/px20"
                        android:gravity="center_vertical"
                        android:text="None"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <ImageView
                        android:id="@+id/shadow_color_none"
                        android:layout_width="@dimen/px16"
                        android:layout_height="@dimen/px20"
                        android:layout_marginStart="@dimen/px20"
                        android:src="@drawable/editorx_text_none_color_choose_icon_selector"
                        app:layout_constraintBottom_toBottomOf="@id/colorbar_shadow_color"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="@id/colorbar_shadow_color" />
                    <com.videoai.aivpcore.editorx.widget.seekbar.ColorBarBgView
                        android:id="@+id/colorbar_shadow_color"
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/px12"
                        app:csb_bg_height="@dimen/px20"
                        app:csb_bg_left_padding="@dimen/px16"
                        app:csb_bg_right_padding="@dimen/px20"
                        app:csb_color_type="b83"
                        app:csb_thumb_border_color="@color/white"
                        app:csb_thumb_border_width="1.0dip"
                        app:csb_thumb_height="@dimen/px24"
                        app:csb_thumb_width="@dimen/px18"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toEndOf="@id/shadow_color_none"
                        app:layout_constraintTop_toBottomOf="@id/shadow_color_title" />
                    <TextView
                        android:id="@+id/tv_shadow_title"
                        android:layout_width="@dimen/px48"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:gravity="center_vertical"
                        android:text="@string/xiaoying_str_blur_background_title"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp12"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toBottomOf="@id/colorbar_shadow_color" />
                    <TextView
                        android:id="@+id/tv_shadow_progress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/px20"
                        android:textColor="@color/color_525252"
                        android:textSize="@dimen/sp12"
                        app:layout_constraintBottom_toBottomOf="@id/tv_shadow_title"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintTop_toTopOf="@id/tv_shadow_title" />
                    <com.videoai.aivpcore.editorx.widget.seekbar.VHSeekBar
                        android:id="@+id/seek_font_shadow"
                        android:layout_width="0.0dip"
                        android:layout_height="@dimen/px20"
                        android:layout_marginTop="@dimen/px12"
                        android:paddingStart="@dimen/px20"
                        android:paddingEnd="20.0dip"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toBottomOf="@id/tv_shadow_title"
                        app:vh_bg_color="@color/app_dcolor_indicator_2"
                        app:vh_bg_radius="@dimen/px20"
                        app:vh_max_progress="100"
                        app:vh_orientation="a30"
                        app:vh_point_color="@color/white"
                        app:vh_point_radius="@dimen/px16"
                        app:vh_progress="0"
                        app:vh_progress_color="@color/colorPrimary"
                        app:vh_progress_radius="20.0dip"
                        app:vh_top_bg_space="@dimen/px20" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
        </ScrollView>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_font"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>
</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent"
    android:gravity="center_vertical"
    android:orientation="vertical"
    android:padding="40.0dip">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="16.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg" />
    <RelativeLayout
        android:id="@+id/hd_layout"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg"
        android:paddingStart="24.0dip"
        android:paddingLeft="24.0dip"
        android:paddingEnd="24.0dip"
        android:paddingRight="24.0dip"
        android:visibility="visible">
        <TextView
            android:id="@+id/purchase_hd_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:clickable="false"
            android:text="@string/xiaoying_str_hd_action_height_720p"
            android:textColor="@color/color_666666"
            android:textSize="14.0sp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="5.0dip"
            android:layout_marginLeft="5.0dip"
            android:layout_marginEnd="5.0dip"
            android:layout_marginRight="5.0dip"
            android:layout_toEndOf="@id/purchase_hd_title"
            android:layout_toRightOf="@id/purchase_hd_title"
            android:background="@drawable/xiaoying_export_hd_flag_bg"
            android:paddingStart="5.0dip"
            android:paddingLeft="5.0dip"
            android:paddingEnd="5.0dip"
            android:paddingRight="5.0dip"
            android:text="@string/xiaoying_str_export_common_hd"
            android:textColor="@color/white"
            android:textSize="12.0sp"
            android:visibility="gone" />
        <ImageView
            android:id="@+id/purchase_hd_lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@null"
            android:src="@drawable/xiaoying_publish_video_export_lock_icon" />
        <TextView
            android:id="@+id/purchase_hd_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/purchase_hd_title"
            android:text="@string/xiaoying_str_publish_export_hd_beta_tip"
            android:textColor="@color/color_666666"
            android:textSize="9.0sp" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/hd_1080_layout"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg"
        android:paddingStart="24.0dip"
        android:paddingLeft="24.0dip"
        android:paddingEnd="24.0dip"
        android:paddingRight="24.0dip"
        android:visibility="visible">
        <TextView
            android:id="@+id/purchase_hd_1080_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:clickable="false"
            android:text="@string/xiaoying_str_hd_action_full_1080p"
            android:textColor="@color/color_666666"
            android:textSize="14.0sp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="5.0dip"
            android:layout_marginLeft="5.0dip"
            android:layout_marginEnd="5.0dip"
            android:layout_marginRight="5.0dip"
            android:layout_toEndOf="@id/purchase_hd_1080_title"
            android:layout_toRightOf="@id/purchase_hd_1080_title"
            android:background="@drawable/xiaoying_export_hd_flag_bg"
            android:paddingStart="5.0dip"
            android:paddingLeft="5.0dip"
            android:paddingEnd="5.0dip"
            android:paddingRight="5.0dip"
            android:text="@string/xiaoying_str_export_common_full_hd"
            android:textColor="@color/white"
            android:textSize="12.0sp"
            android:visibility="gone" />
        <ImageView
            android:id="@+id/purchase_hd_1080_lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@null"
            android:src="@drawable/xiaoying_publish_video_export_lock_icon" />
        <TextView
            android:id="@+id/purchase_hd_1080_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/purchase_hd_1080_title"
            android:clickable="false"
            android:text="@string/xiaoying_str_publish_export_hd_beta_tip"
            android:textColor="@color/color_666666"
            android:textSize="9.0sp" />
    </RelativeLayout>
    <TextView
        android:id="@+id/normal_layout"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg"
        android:gravity="center_vertical"
        android:paddingStart="24.0dip"
        android:paddingLeft="24.0dip"
        android:paddingEnd="24.0dip"
        android:paddingRight="24.0dip"
        android:text="@string/xiaoying_str_hd_action_normal_480p"
        android:textColor="@color/color_666666"
        android:textSize="14.0sp" />
    <TextView
        android:id="@+id/extra_layout"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg"
        android:gravity="center_vertical"
        android:paddingStart="24.0dip"
        android:paddingLeft="24.0dip"
        android:paddingEnd="24.0dip"
        android:paddingRight="24.0dip"
        android:text="@string/xiaoying_str_free_to_export_once"
        android:textColor="@color/color_666666"
        android:textSize="14.0sp" />
    <TextView
        android:id="@+id/gif_layout"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg"
        android:gravity="center_vertical"
        android:paddingStart="24.0dip"
        android:paddingLeft="24.0dip"
        android:paddingEnd="24.0dip"
        android:paddingRight="24.0dip"
        android:text="@string/xiaoying_str_gif_action_item"
        android:textColor="@color/color_666666"
        android:textDirection="locale"
        android:textSize="14.0sp" />
    <RelativeLayout
        android:id="@+id/hd_2k_layout"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg"
        android:paddingStart="24.0dip"
        android:paddingLeft="24.0dip"
        android:paddingEnd="24.0dip"
        android:paddingRight="24.0dip"
        android:visibility="visible">
        <TextView
            android:id="@+id/purchase_hd_2k_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:clickable="false"
            android:text="@string/viva_publish_2k_item_title"
            android:textColor="@color/color_666666"
            android:textSize="14.0sp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="10.0dip"
            android:layout_marginLeft="10.0dip"
            android:layout_marginEnd="10.0dip"
            android:layout_marginRight="10.0dip"
            android:layout_toEndOf="@id/purchase_hd_2k_title"
            android:layout_toRightOf="@id/purchase_hd_2k_title"
            android:background="@drawable/xiaoying_export_hd_flag_bg"
            android:paddingStart="5.0dip"
            android:paddingLeft="5.0dip"
            android:paddingEnd="5.0dip"
            android:paddingRight="5.0dip"
            android:text="New"
            android:textColor="@color/white"
            android:textSize="12.0sp"
            android:visibility="gone" />
        <ImageView
            android:id="@+id/purchase_hd_2k_lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@null"
            android:src="@drawable/xiaoying_publish_video_export_lock_icon" />
        <TextView
            android:id="@+id/purchase_hd_2k_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/purchase_hd_2k_title"
            android:clickable="false"
            android:text="@string/xiaoying_str_publish_export_hd_beta_tip"
            android:textColor="@color/color_666666"
            android:textSize="9.0sp" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/hd_4k_layout"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg"
        android:paddingStart="24.0dip"
        android:paddingLeft="24.0dip"
        android:paddingEnd="24.0dip"
        android:paddingRight="24.0dip"
        android:visibility="visible">
        <TextView
            android:id="@+id/purchase_hd_4k_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:clickable="false"
            android:text="@string/viva_publish_4k_item_title"
            android:textColor="@color/color_666666"
            android:textSize="14.0sp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="10.0dip"
            android:layout_marginLeft="10.0dip"
            android:layout_marginEnd="10.0dip"
            android:layout_marginRight="10.0dip"
            android:layout_toEndOf="@id/purchase_hd_4k_title"
            android:layout_toRightOf="@id/purchase_hd_4k_title"
            android:background="@drawable/xiaoying_export_hd_flag_bg"
            android:paddingStart="5.0dip"
            android:paddingLeft="5.0dip"
            android:paddingEnd="5.0dip"
            android:paddingRight="5.0dip"
            android:text="New"
            android:textColor="@color/white"
            android:textSize="12.0sp"
            android:visibility="gone" />
        <ImageView
            android:id="@+id/purchase_hd_4k_lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@null"
            android:src="@drawable/xiaoying_publish_video_export_lock_icon" />
        <TextView
            android:id="@+id/purchase_hd_4k_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/purchase_hd_4k_title"
            android:clickable="false"
            android:text="@string/xiaoying_str_publish_export_hd_beta_tip"
            android:textColor="@color/color_666666"
            android:textSize="9.0sp" />
    </RelativeLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="16.0dip"
        android:background="@drawable/editor_selector_pub_dialog_list_item_bg" />
</LinearLayout>
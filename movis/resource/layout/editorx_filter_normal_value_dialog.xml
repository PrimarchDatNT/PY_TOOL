<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/layout_filter_normal_value_root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/transparent"
    android:orientation="vertical">
    <View
        android:id="@id/filter_normal_bg"
        android:layout_width="match_parent"
        android:layout_height="90.0dip"
        android:layout_marginTop="38.0dip"
        android:background="@drawable/editorx_shape_audio_volume_dialog_bg"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.aivpcore.editorx.widget.seekbar.PopSeekBar
        android:id="@id/filter_normal_seek_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent"
        app:psb_bg_color="@color/default_background_pb_progress_color"
        app:psb_max_progress="10"
        app:psb_seek_bar_progress_drawable="@drawable/psb_progress_drawable"
        app:psb_seek_bar_thumb="@drawable/psb_selector_seek_bar_thumb"
        app:psb_text_bg_space="24.0dip"
        app:psb_top_text_color="@color/color_333333"
        app:psb_top_text_diam="44.0dip"
        app:psb_top_text_size="@dimen/sp16" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50.0dip"
        android:text="@string/xiaoying_str_ve_effect_title"
        android:textColor="@color/white"
        android:textSize="@dimen/sp14"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
</androidx.constraintlayout.widget.ConstraintLayout>
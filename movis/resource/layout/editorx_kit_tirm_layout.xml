<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@id/ll_tab"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px270"
        android:layout_gravity="bottom"
        android:background="@color/app_dcolor_background"
        android:clickable="true"
        android:focusable="true"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@id/rl_tittle_text"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px48"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toTopOf="parrent">
            <TextView
                android:id="@id/tv_kit_trim_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="@string/xiaoying_str_ve_gallery_filter_menu_item_photo_only"
                android:textColor="@drawable/editorx_selector_filter_tab_title"
                android:textSize="@dimen/sp14"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/px1"
                android:background="@color/app_dcolor_indicator"
                app:layout_constraintBottom_toBottomOf="parrent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@id/cl_kit_trim_bar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px56"
            android:background="@color/app_dcolor_bg_1"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parrent"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent">
            <TextView
                android:id="@id/tv_kit_trim_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginStart="@dimen/px16"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="@dimen/sp12"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
            <ImageView
                android:id="@id/iv_bar_done"
                android:layout_width="@dimen/px56"
                android:layout_height="@dimen/px56"
                android:layout_centerVertical="true"
                android:background="@drawable/btn_dcolor_ripple"
                android:scaleType="centerInside"
                android:src="@drawable/editor_selector_btn_ok"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@id/rl_children"
            android:layout_width="0.0dip"
            android:layout_height="0.0dip"
            android:background="@color/app_dcolor_background"
            app:layout_constraintBottom_toTopOf="@id/cl_kit_trim_bar"
            app:layout_constraintEnd_toEndOf="parrent"
            app:layout_constraintStart_toStartOf="parrent"
            app:layout_constraintTop_toBottomOf="@id/rl_tittle_text">
            <com.videoai.aivpcore.timeline.fixed.trim.TrimTimelineForKit
                android:id="@id/trimTimeline"
                android:layout_width="match_parent"
                android:layout_height="200.0dip"
                app:layout_constraintTop_toTopOf="parrent" />
            <ImageView
                android:id="@id/iv_kit_trim_pic_cover"
                android:layout_width="@dimen/px64"
                android:layout_height="@dimen/px64"
                android:layout_marginTop="58.0dip"
                android:scaleType="centerCrop"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
            <TextView
                android:id="@id/tv_kit_trim_pic_cover"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4.0dip"
                android:textColor="@color/white"
                android:textSize="@dimen/sp10"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toBottomOf="@id/iv_kit_trim_pic_cover" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
        android:id="@id/btnScale"
        android:layout_width="@dimen/px64"
        android:layout_height="@dimen/px64"
        android:layout_marginBottom="6.0dip"
        app:layout_constraintBottom_toTopOf="@id/ll_tab"
        app:layout_constraintEnd_toEndOf="parrent"
        app:sitv_bottom_text="@string/xiaoying_str_ve_basic__enlarge_title"
        app:sitv_top_drawable="@drawable/editorx_btn_clip_scale" />
    <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
        android:id="@id/btnRotate"
        android:layout_width="@dimen/px64"
        android:layout_height="@dimen/px64"
        android:layout_marginBottom="20.0dip"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@id/btnScale"
        app:layout_constraintEnd_toEndOf="parrent"
        app:sitv_bottom_text="@string/xiaoying_str_ve_basic_rotate_title"
        app:sitv_top_drawable="@drawable/editorx_btn_clip_rotate" />
    <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
        android:id="@id/sitv_mirror"
        android:layout_width="@dimen/px64"
        android:layout_height="@dimen/px64"
        android:layout_marginBottom="20.0dip"
        app:layout_constraintBottom_toTopOf="@id/btnRotate"
        app:layout_constraintEnd_toEndOf="parrent"
        app:sitv_bottom_text="@string/xiaoying_str_ve_basic_mirror_title"
        app:sitv_top_drawable="@drawable/editorx_btn_clip_mirror" />
    <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
        android:id="@id/btnReset"
        android:layout_width="@dimen/px64"
        android:layout_height="@dimen/px64"
        android:layout_marginBottom="20.0dip"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@id/sitv_mirror"
        app:layout_constraintEnd_toEndOf="parrent"
        app:sitv_bottom_text="@string/xiaoying_str_edit_reset_text"
        app:sitv_top_drawable="@drawable/editorx_ico_effect_reset" />
    <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
        android:id="@id/btnRotateL"
        android:layout_width="@dimen/px64"
        android:layout_height="@dimen/px64"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@id/btnScale"
        app:layout_constraintTop_toTopOf="@id/btnScale"
        app:sitv_bottom_text="@string/xiaoying_str_ve_basic_rotate_title"
        app:sitv_top_drawable="@drawable/editorx_btn_clip_rotate" />
    <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
        android:id="@id/sitv_mirrorL"
        android:layout_width="@dimen/px64"
        android:layout_height="@dimen/px64"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@id/btnRotateL"
        app:layout_constraintTop_toTopOf="@id/btnRotateL"
        app:sitv_bottom_text="@string/xiaoying_str_ve_basic_mirror_title"
        app:sitv_top_drawable="@drawable/editorx_btn_clip_mirror" />
    <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
        android:id="@id/btnResetL"
        android:layout_width="@dimen/px64"
        android:layout_height="@dimen/px64"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@id/sitv_mirrorL"
        app:layout_constraintTop_toTopOf="@id/sitv_mirrorL"
        app:sitv_bottom_text="@string/xiaoying_str_edit_reset_text"
        app:sitv_top_drawable="@drawable/editorx_ico_effect_reset" />
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60.0dip">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@id/awn"
            android:layout_width="fill_parent"
            android:layout_height="300.0dip"
            android:background="#4400ffff"
            android:visibility="gone">
            <TextView
                android:id="@id/bfi"
                android:layout_width="wrap_content"
                android:layout_height="40.0dip"
                android:layout_marginStart="50.0dip"
                android:background="#ffffff00"
                android:gravity="center"
                android:paddingLeft="5.0dip"
                android:paddingRight="5.0dip"
                android:text="jump kit"
                android:textColor="#ff000000"
                app:layout_constraintStart_toStartOf="apv"
                app:layout_constraintTop_toTopOf="apv" />
            <TextView
                android:id="@id/bfh"
                android:layout_width="60.0dip"
                android:layout_height="40.0dip"
                android:background="#ffffff00"
                android:gravity="center"
                android:paddingLeft="5.0dip"
                android:paddingRight="5.0dip"
                android:text="export"
                android:textColor="#ff000000"
                app:layout_constraintEnd_toEndOf="apv"
                app:layout_constraintTop_toTopOf="apv" />
            <TextView
                android:id="@id/bfg"
                android:layout_width="60.0dip"
                android:layout_height="60.0dip"
                android:background="#ffffff00"
                android:gravity="center"
                android:paddingLeft="5.0dip"
                android:paddingRight="5.0dip"
                android:text="lock"
                android:textColor="#ff000000"
                app:layout_constraintEnd_toEndOf="apv"
                app:layout_constraintTop_toTopOf="@id/bff" />
            <TextView
                android:id="@id/bff"
                android:layout_width="0.0dip"
                android:layout_height="60.0dip"
                android:layout_marginTop="10.0dip"
                android:background="#ff222222"
                android:gravity="center|left"
                android:paddingLeft="5.0dip"
                android:paddingRight="5.0dip"
                android:textColor="#ffffffff"
                app:layout_constraintEnd_toStartOf="@id/bfg"
                app:layout_constraintStart_toStartOf="apv"
                app:layout_constraintTop_toBottomOf="@id/bfi" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <TextView
            android:id="@id/bke"
            android:layout_width="40.0dip"
            android:layout_height="40.0dip"
            android:background="#ffffff00"
            android:gravity="center"
            android:text="Q"
            android:textColor="#ff000000" />
    </RelativeLayout>
</FrameLayout>
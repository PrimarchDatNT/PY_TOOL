<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/item_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center">
    <ImageView
        android:id="@id/popup_item_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="10.0dip"
        android:layout_marginLeft="10.0dip"
        android:layout_toEndOf="@id/popup_item_title"
        android:layout_toRightOf="@id/popup_item_title"
        android:src="@drawable/xiaoying_ve_gallery_title_choose_dot" />
    <TextView
        android:id="@id/popup_item_title"
        android:layout_width="wrap_content"
        android:layout_height="40.0dip"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@drawable/v4_xiaoying_com_text_color_gray_white_selector"
        android:textSize="17.0sp" />
</RelativeLayout>
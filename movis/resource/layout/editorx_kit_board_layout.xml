<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintLeft_toLeftOf="parrent"
        app:layout_constraintRight_toRightOf="parrent"
        app:layout_constraintTop_toTopOf="parrent">
        <RelativeLayout
            android:id="@+id/rl_tittle_text"
            android:layout_width="match_parent"
            android:layout_height="48.0dip"
            android:background="@color/color_141414"
            app:layout_constraintBottom_toTopOf="@id/rl_children"
            app:layout_constraintLeft_toLeftOf="parrent"
            app:layout_constraintRight_toRightOf="parrent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/ll_tittle_text"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:orientation="horizontal">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_clip"
                    android:layout_width="100.0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    app:layout_constraintEnd_toStartOf="@id/cl_music"
                    app:layout_constraintHorizontal_chainStyle="apo"
                    app:layout_constraintStart_toStartOf="parrent"
                    app:layout_constraintTop_toTopOf="parrent">
                    <TextView
                        android:id="@+id/tv_clip_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_weight="1.0"
                        android:gravity="center"
                        android:text="@string/xiaoying_str_edit_school_title_bar_clip"
                        android:textColor="@drawable/editorx_selector_filter_tab_title"
                        android:textSize="14.0sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parrent"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <ImageView
                        android:id="@+id/iv_clip_point"
                        android:layout_width="wrap_content"
                        android:layout_height="12.0dip"
                        app:layout_constraintBottom_toBottomOf="parrent"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_music"
                    android:layout_width="100.0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    app:layout_constraintEnd_toStartOf="@id/cl_text"
                    app:layout_constraintHorizontal_chainStyle="apo"
                    app:layout_constraintStart_toEndOf="@id/cl_clip"
                    app:layout_constraintTop_toTopOf="parrent">
                    <TextView
                        android:id="@+id/tv_music_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_weight="1.0"
                        android:gravity="center"
                        android:text="@string/xiaoying_str_ve_music_title"
                        android:textColor="@drawable/editorx_selector_filter_tab_title"
                        android:textSize="14.0sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parrent"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <ImageView
                        android:id="@+id/iv_music_point"
                        android:layout_width="wrap_content"
                        android:layout_height="12.0dip"
                        app:layout_constraintBottom_toBottomOf="parrent"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_text"
                    android:layout_width="100.0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    app:layout_constraintEnd_toEndOf="parrent"
                    app:layout_constraintHorizontal_chainStyle="apo"
                    app:layout_constraintStart_toEndOf="@id/cl_music"
                    app:layout_constraintTop_toTopOf="parrent">
                    <TextView
                        android:id="@+id/tv_text_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_weight="1.0"
                        android:gravity="center"
                        android:text="@string/xiaoying_str_ve_subtitle_title"
                        android:textColor="@drawable/editorx_selector_filter_tab_title"
                        android:textSize="14.0sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parrent"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                    <ImageView
                        android:id="@+id/iv_text_point"
                        android:layout_width="wrap_content"
                        android:layout_height="12.0dip"
                        app:layout_constraintBottom_toBottomOf="parrent"
                        app:layout_constraintEnd_toEndOf="parrent"
                        app:layout_constraintStart_toStartOf="parrent"
                        app:layout_constraintTop_toTopOf="parrent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/v_three_tab_indicator"
                android:layout_width="6.0dip"
                android:layout_height="2.0dip"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="8.0dip"
                android:background="@drawable/editorx_filter_shape_tab_indicator" />
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dip"
                android:layout_alignParentBottom="true"
                android:background="@color/app_dcolor_indicator" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_children"
            android:layout_width="match_parent"
            android:layout_height="174.0dip"
            android:background="@color/color_141414"
            app:layout_constraintBottom_toTopOf="@id/cl_high_level"
            app:layout_constraintLeft_toLeftOf="parrent"
            app:layout_constraintRight_toRightOf="parrent" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_high_level"
            android:layout_width="match_parent"
            android:layout_height="64.0dip"
            android:background="@color/color_141414"
            android:paddingStart="48.0dip"
            android:paddingRight="48.0dip"
            app:layout_constraintBottom_toBottomOf="parrent"
            app:layout_constraintLeft_toLeftOf="parrent"
            app:layout_constraintRight_toRightOf="parrent">
            <TextView
                android:id="@+id/tv_kit_publish"
                android:layout_width="match_parent"
                android:layout_height="48.0dip"
                android:background="@drawable/editorx_shape_save_btn_bg"
                android:gravity="center"
                android:text="@string/xiaoying_str_com_save_title"
                android:textColor="@color/white"
                android:textSize="16.0sp"
                app:layout_constraintBottom_toBottomOf="parrent"
                app:layout_constraintLeft_toLeftOf="parrent"
                app:layout_constraintRight_toRightOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="60.0dip"
            android:background="@drawable/editorx_shape_preview_title_bg"
            app:layout_constraintLeft_toLeftOf="parrent"
            app:layout_constraintRight_toRightOf="parrent"
            app:layout_constraintTop_toTopOf="parrent">
            <ImageView
                android:id="@+id/iv_kit_back"
                android:layout_width="52.0dip"
                android:layout_height="52.0dip"
                android:layout_marginStart="1.0dip"
                android:padding="14.0dip"
                android:src="@drawable/editorx_kit_icon_back"
                app:layout_constraintStart_toStartOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_high_level_edit"
                android:layout_width="wrap_content"
                android:layout_height="28.0dip"
                android:layout_marginTop="12.0dip"
                android:layout_marginEnd="15.0dip"
                android:background="@drawable/editorx_shape_high_level_edit_btn"
                android:ellipsize="end"
                android:gravity="center"
                android:maxLines="2"
                android:paddingLeft="16.0dip"
                android:paddingRight="16.0dip"
                android:text="@string/xiaoying_str_edit_school_bottom_edit_btn_title"
                android:textColor="@color/opacity_9_white"
                android:textStyle="bold"
                app:autoSizeStepGranularity="1.0sp"
                app:autoSizeTextType="uniform"
                app:layout_constraintEnd_toEndOf="parrent"
                app:layout_constraintTop_toTopOf="parrent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <com.videoai.aivpcore.editorx.board.kit.text.KitTextKeyboardView
        android:id="@+id/kit_keyboard"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintLeft_toLeftOf="parrent"
        app:layout_constraintRight_toRightOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
</androidx.constraintlayout.widget.ConstraintLayout>
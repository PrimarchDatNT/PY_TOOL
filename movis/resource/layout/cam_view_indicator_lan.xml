<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:rotateView="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_centerVertical="true"
        android:background="@drawable/azt"
        android:gravity="center"
        android:orientation="horizontal">
        <com.videoai.aivpcore.ui.view.RotateImageView
            android:id="@id/a5q"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:src="@drawable/ath"
            rotateView:rotation="270" />
        <com.videoai.aivpcore.ui.view.RotateImageView
            android:id="@id/a5s"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:src="@drawable/atr"
            rotateView:rotation="270" />
        <com.videoai.aivpcore.ui.view.RotateImageView
            android:id="@id/a5l"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            rotateView:rotation="270" />
        <com.videoai.aivpcore.ui.view.RotateImageView
            android:id="@id/a5a"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:src="@drawable/at9"
            rotateView:rotation="270" />
        <ImageView
            android:id="@id/mv"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:scaleType="centerInside"
            android:src="@drawable/asm" />
    </LinearLayout>
</RelativeLayout>
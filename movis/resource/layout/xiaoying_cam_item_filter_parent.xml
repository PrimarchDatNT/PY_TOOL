<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="82.0dip"
    android:layout_height="wrap_content"
    android:clipToPadding="false"
    android:paddingStart="5.0dip"
    android:paddingLeft="5.0dip"
    android:paddingTop="5.0dip"
    android:paddingEnd="5.0dip"
    android:paddingRight="5.0dip">
    <com.videoai.aivpcore.xyui.RoundCornerImageView
        android:id="@id/item_fitler_parent_cover"
        android:layout_width="71.0dip"
        android:layout_height="71.0dip"
        android:layout_marginStart="0.5dip"
        android:layout_marginLeft="0.5dip"
        android:layout_marginTop="0.5dip"
        android:scaleType="centerCrop"
        android:src="@color/black"
        app:cornerRadius="2.0dip" />
    <ImageView
        android:id="@id/icon_fitler_parent_new"
        android:layout_width="25.0dip"
        android:layout_height="15.0dip"
        android:layout_alignTop="@id/item_fitler_parent_cover"
        android:layout_alignEnd="@id/item_fitler_parent_cover"
        android:layout_alignRight="@id/item_fitler_parent_cover"
        android:layout_marginTop="2.0dip"
        android:layout_marginEnd="2.0dip"
        android:layout_marginRight="2.0dip"
        android:src="@drawable/xiaoying_cam_icon_filter_new"
        android:visibility="gone" />
    <ImageView
        android:id="@id/bg_filter_single_selected"
        android:layout_width="72.0dip"
        android:layout_height="72.0dip"
        android:src="@drawable/xiaoying_cam_bg_filter_single_selected"
        android:visibility="gone" />
    <ImageView
        android:id="@id/iv_filter_download"
        android:layout_width="15.0dip"
        android:layout_height="15.0dip"
        android:layout_alignEnd="@id/item_fitler_parent_cover"
        android:layout_alignRight="@id/item_fitler_parent_cover"
        android:layout_alignBottom="@id/item_fitler_parent_cover"
        android:layout_marginEnd="5.0dip"
        android:layout_marginRight="5.0dip"
        android:layout_marginBottom="5.0dip"
        android:src="@drawable/editor_effect_download_tiny_icon" />
    <LinearLayout
        android:id="@id/layout_download_progress"
        android:layout_width="72.0dip"
        android:layout_height="72.0dip"
        android:background="@drawable/xiaoying_cam_bg_filter_download"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="invisible">
        <com.videoai.mobile.component.imageview.DynamicLoadingImageView
            android:id="@id/bg_download_progress"
            android:layout_width="20.0dip"
            android:layout_height="20.0dip" />
        <TextView
            android:id="@id/text_download_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8.0dip"
            android:text="0%"
            android:textColor="#ffffffff"
            android:textSize="12.0sp" />
    </LinearLayout>
    <RelativeLayout
        android:id="@id/bg_filter_group_revoke"
        android:layout_width="72.0dip"
        android:layout_height="72.0dip"
        android:background="@drawable/xiaoying_cam_bg_filter_group_revoke"
        android:visibility="gone">
        <ImageView
            android:layout_width="44.0dip"
            android:layout_height="44.0dip"
            android:layout_centerInParent="true"
            android:src="@drawable/xiaoying_cam_icon_filter_group_revoke" />
    </RelativeLayout>
    <TextView
        android:id="@id/item_fitler_parent_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/item_fitler_parent_cover"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5.0dip"
        android:lines="1"
        android:text="Name"
        android:textColor="@color/color_8E8E93"
        android:textSize="11.0sp" />
    <RelativeLayout
        android:id="@id/layout_filter_lock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="-5.0dip"
        android:layout_marginEnd="-5.0dip"
        android:layout_marginRight="-5.0dip"
        android:visibility="gone">
        <ImageView
            android:id="@id/icon_filter_lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>
</RelativeLayout>
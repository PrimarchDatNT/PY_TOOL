<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_dcolor_background">
    <androidx.recyclerview.widget.RecyclerView
        android:id="@id/anim_tab_recyclerview"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/px42"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.aivpcore.common.ui.RtlViewPager
        android:id="@id/anim_view_pager"
        android:layout_width="match_parent"
        android:layout_height="0.0dip"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/anim_tab_recyclerview" />
    <com.videoai.aivpcore.editorx.board.effect.subtitle2.SubtitleAnimStateView
        android:id="@id/anim_state_view"
        android:layout_width="match_parent"
        android:layout_height="0.0dip"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/anim_view_pager"
        app:layout_constraintEnd_toEndOf="@id/anim_view_pager"
        app:layout_constraintStart_toStartOf="@id/anim_view_pager"
        app:layout_constraintTop_toTopOf="@id/anim_view_pager" />
</merge>
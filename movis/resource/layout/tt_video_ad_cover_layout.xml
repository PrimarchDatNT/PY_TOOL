<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tt_video_ad_covers"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/tt_trans_half_black"
    android:visibility="gone">
    <ImageView
        android:id="@+id/tt_video_ad_finish_cover_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:tint="@color/tt_ssxinmian15"
        android:tintMode="src_over" />
    <RelativeLayout
        android:id="@+id/tt_video_ad_cover_center_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">
        <com.bytedance.sdk.openadsdk.core.widget.CornerIV
            android:id="@+id/tt_video_ad_logo_image"
            android:layout_width="44.0dip"
            android:layout_height="44.0dip"
            android:layout_centerHorizontal="true"
            android:scaleType="centerInside" />
        <TextView
            android:id="@+id/tt_video_btn_ad_image_tv"
            android:layout_width="44.0dip"
            android:layout_height="44.0dip"
            android:layout_alignStart="@id/tt_video_ad_logo_image"
            android:layout_alignLeft="@id/tt_video_ad_logo_image"
            android:layout_alignTop="@id/tt_video_ad_logo_image"
            android:layout_alignEnd="@id/tt_video_ad_logo_image"
            android:layout_alignRight="@id/tt_video_ad_logo_image"
            android:layout_alignBottom="@id/tt_video_ad_logo_image"
            android:layout_centerHorizontal="true"
            android:background="@drawable/tt_circle_solid_mian"
            android:gravity="center"
            android:textColor="@color/tt_white"
            android:textSize="19.0sp"
            android:textStyle="bold"
            android:visibility="gone" />
        <TextView
            android:id="@+id/tt_video_ad_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tt_video_ad_logo_image"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="6.0dip"
            android:ellipsize="end"
            android:maxLines="1"
            android:textColor="@color/tt_white"
            android:textSize="12.0sp" />
        <TextView
            android:id="@+id/tt_video_ad_button"
            android:layout_width="100.0dip"
            android:layout_height="28.0dip"
            android:layout_below="@id/tt_video_ad_name"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="20.0dip"
            android:background="@drawable/tt_ad_cover_btn_begin_bg"
            android:ellipsize="end"
            android:gravity="center"
            android:maxLines="1"
            android:minWidth="72.0dip"
            android:paddingLeft="10.0dip"
            android:paddingTop="2.0dip"
            android:paddingRight="10.0dip"
            android:paddingBottom="2.0dip"
            android:textColor="@color/tt_white"
            android:textSize="14.0sp"
            android:visibility="gone" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/tt_video_ad_cover_center_layout_draw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:visibility="gone">
        <TextView
            android:id="@+id/tt_video_ad_button_draw"
            android:layout_width="580.0px"
            android:layout_height="98.0px"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="20.0dip"
            android:background="@drawable/tt_ad_cover_btn_draw_begin_bg"
            android:gravity="center"
            android:padding="2.0dip"
            android:text="@string/tt_video_download_apk"
            android:textColor="@color/tt_white"
            android:textSize="34.0px"
            android:visibility="gone" />
        <TextView
            android:id="@+id/tt_video_ad_replay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tt_video_ad_button_draw"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="60.0px"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="replay"
            android:textColor="@color/tt_white"
            android:textSize="34.0px" />
    </RelativeLayout>
</RelativeLayout>
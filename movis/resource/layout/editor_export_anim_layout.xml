<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:attr="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@id/view_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/default_app_color"
        android:visibility="visible" />
    <ImageView
        android:id="@id/center_baseline"
        android:layout_width="match_parent"
        android:layout_height="1.0dip"
        android:layout_centerVertical="true"
        android:visibility="visible" />
    <RelativeLayout
        android:id="@id/textview_hint_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/center_baseline"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="65.0dip">
        <TextView
            android:id="@id/textview_hint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="@string/xiaoying_str_ve_video_import_progressing"
            android:textColor="@android:color/white"
            android:textSize="15.0sp"
            android:visibility="invisible" />
        <TextView
            android:id="@id/textview_hint2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="@string/xiaoying_str_ve_msg_video_or_prj_export_success"
            android:textColor="@android:color/white"
            android:textSize="15.0sp"
            android:visibility="invisible" />
        <TextView
            android:id="@id/textview_hint3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="@string/xiaoying_str_ve_msg_video_or_prj_export_failed"
            android:textColor="@color/white"
            android:textSize="15.0sp"
            android:visibility="invisible" />
        <TextView
            android:id="@id/textview_hint4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30.0dip"
            android:gravity="center"
            android:paddingStart="10.0dip"
            android:paddingLeft="10.0dip"
            android:paddingEnd="10.0dip"
            android:paddingRight="10.0dip"
            android:text="@string/xiaoying_str_prj_export_exporting_tips"
            android:textColor="@color/white"
            android:textSize="15.0sp"
            android:visibility="invisible" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="154.0dip"
        android:layout_height="154.0dip"
        android:layout_above="@id/center_baseline"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="-35.0dip"
        android:gravity="center">
        <ImageView
            android:id="@id/imgview_progress"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <TextView
            android:id="@id/textview_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@android:color/white"
            android:visibility="invisible" />
    </RelativeLayout>
    <com.videoai.aivpcore.xyui.RoundedTextView
        android:id="@id/btn_cancel"
        android:layout_width="match_parent"
        android:layout_height="44.0dip"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="10.0dip"
        android:layout_marginLeft="10.0dip"
        android:layout_marginEnd="10.0dip"
        android:layout_marginRight="10.0dip"
        android:layout_marginBottom="10.0dip"
        android:gravity="center"
        android:text="@string/xiaoying_str_com_cancel"
        android:textColor="@android:color/white"
        android:textSize="15.0sp"
        android:visibility="invisible"
        attr:VS_CornerRadius="5.0dip"
        attr:VS_SolidColor="@android:color/transparent"
        attr:VS_StrokeColor="@android:color/white"
        attr:VS_StrokeWidth="1.0dip" />
</RelativeLayout>
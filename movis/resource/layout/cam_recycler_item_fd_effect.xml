<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:attr="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:id="@id/main_layout"
        android:layout_width="70.0dip"
        android:layout_height="70.0dip">
        <com.videoai.mobile.component.imageview.DynamicLoadingImageView
            android:id="@id/img_filter_thumb"
            android:layout_width="45.0dip"
            android:layout_height="45.0dip"
            android:layout_centerInParent="true"
            attr:actualImageScaleType="o2"
            attr:roundedCornerRadius="2.0dip" />
        <ImageView
            android:id="@id/img_filter_focus"
            android:layout_width="60.0dip"
            android:layout_height="60.0dip"
            android:layout_centerInParent="true"
            android:src="@drawable/xiaoying_cam_bg_filter_single_selected" />
        <com.videoai.mobile.component.imageview.DynamicLoadingImageView
            android:id="@id/download_progress"
            android:layout_width="20.0dip"
            android:layout_height="20.0dip"
            android:layout_centerInParent="true"
            android:visibility="gone" />
        <ImageView
            android:id="@id/img_new_flag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="10.0dip"
            android:layout_marginEnd="8.0dip"
            android:layout_marginRight="8.0dip"
            android:scaleType="fitXY"
            android:src="@drawable/xiaoying_camera_label_new"
            android:visibility="gone" />
        <ImageView
            android:id="@id/img_download_flag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="8.0dip"
            android:layout_marginRight="8.0dip"
            android:layout_marginBottom="10.0dip"
            android:scaleType="fitXY"
            android:src="@drawable/xiaoying_camera_icon_downloadsticker"
            android:visibility="visible" />
        <ImageView
            android:id="@id/img_audio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="8.0dip"
            android:layout_marginLeft="8.0dip"
            android:layout_marginBottom="10.0dip"
            android:scaleType="fitXY"
            android:src="@drawable/cam_icon_paster_audio"
            android:visibility="gone" />
        <ImageView
            android:id="@id/img_lock_flag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="8.0dip"
            android:layout_marginRight="8.0dip"
            android:layout_marginBottom="10.0dip"
            android:scaleType="fitXY"
            android:src="@drawable/xiaoying_camera_icon_lock"
            android:visibility="gone" />
    </RelativeLayout>
</RelativeLayout>
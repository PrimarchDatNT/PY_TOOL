<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@id/iv_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26.0dip"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent"
        app:srcCompat="@drawable/rate_sticker_star_3" />
    <TextView
        android:id="@id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30.0dip"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/title_cancel_feedback"
        android:textColor="@color/black"
        android:textSize="16.0sp"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/iv_cancel" />
    <TextView
        android:id="@id/tv_content_feedback"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16.0dip"
        android:layout_marginTop="16.0dip"
        android:layout_marginEnd="16.0dip"
        android:gravity="center"
        android:text="@string/content_cancel_feedback"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/tv_title" />
    <TextView
        android:id="@id/btn_cancel"
        android:layout_width="0.0dip"
        android:layout_height="50.0dip"
        android:layout_marginStart="16.0dip"
        android:layout_marginTop="24.0dip"
        android:layout_marginEnd="16.0dip"
        android:layout_marginBottom="12.0dip"
        android:background="?a5d"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:text="@string/cancel_feedback"
        android:textColor="@color/black"
        android:textSize="16.0sp"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toStartOf="@id/btn_continue"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/tv_content_feedback" />
    <TextView
        android:id="@id/btn_continue"
        android:layout_width="0.0dip"
        android:layout_height="50.0dip"
        android:layout_marginTop="24.0dip"
        android:layout_marginEnd="16.0dip"
        android:layout_marginBottom="12.0dip"
        android:background="?a5d"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:text="@string/continue_feedback"
        android:textColor="@color/colorPrimary"
        android:textSize="16.0sp"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toEndOf="@id/btn_cancel"
        app:layout_constraintTop_toBottomOf="@id/tv_content_feedback" />
</androidx.constraintlayout.widget.ConstraintLayout>
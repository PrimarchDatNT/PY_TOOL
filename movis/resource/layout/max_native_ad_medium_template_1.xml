<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:minWidth="300.0dip"
    android:minHeight="250.0dip"
    android:paddingBottom="@dimen/dj">
    <LinearLayout
        android:id="@id/ez"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_margin="@dimen/dj"
        android:layout_toStartOf="@id/fc"
        android:layout_toLeftOf="@id/fc"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <FrameLayout
            android:id="@id/f8"
            style="@style/jz"
            android:layout_marginEnd="@dimen/dj"
            android:layout_marginRight="@dimen/dj">
            <ImageView
                android:id="@id/f7"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:adjustViewBounds="true"
                android:scaleType="fitCenter" />
        </FrameLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView style="@style/com.applovin.mediation.nativeAds.MaxNativeAdView.SmallAdBadgeTextView" />
                <TextView
                    android:id="@id/fd"
                    style="@style/com.applovin.mediation.nativeAds.MaxNativeAdView.SmallScrollingTitleTextView" />
            </LinearLayout>
            <TextView
                android:id="@id/f1"
                style="@style/com.applovin.mediation.nativeAds.MaxNativeAdView.SmallScrollingBodyTextView" />
        </LinearLayout>
    </LinearLayout>
    <FrameLayout
        android:id="@id/fc"
        android:layout_width="wrap_content"
        android:layout_height="20.0dip"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="2.0dip"
        android:layout_marginEnd="2.0dip"
        android:layout_marginRight="2.0dip"
        android:minWidth="20.0dip" />
    <TextView
        android:id="@id/f3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/ez"
        android:layout_marginLeft="8.0dip"
        android:layout_marginRight="8.0dip"
        android:layout_marginBottom="8.0dip"
        android:maxLines="3"
        android:textSize="12.0sp" />
    <FrameLayout
        android:id="@id/fb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/f4"
        android:layout_below="@id/f3"
        android:layout_centerHorizontal="true" />
    <Button
        android:id="@id/f4"
        style="@style/com.applovin.mediation.nativeAds.MaxNativeAdView.CTAButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_margin="@dimen/dj"
        android:singleLine="false"
        android:textSize="15.0sp" />
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="false">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@id/imgview_center_divider"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px1"
        android:layout_centerInParent="true" />
    <TextView
        android:id="@id/tv_export_failed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imgview_center_divider"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="@string/xiaoying_str_ve_msg_video_or_prj_export_failed"
        android:textColor="@color/white"
        android:textSize="16.0sp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_export_failed"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20.0dip"
        android:gravity="center"
        android:text="@string/xiaoying_str_ve_msg_video_or_prj_export_failed_and_retry"
        android:textColor="@color/white"
        android:textSize="14.0sp" />
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/imgview_center_divider"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="55.0dip"
        android:background="@drawable/comm_studio_self_videos_empty_bg" />
    <RelativeLayout
        android:id="@id/layout_normal_retry"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/imgview_center_divider">
        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@id/imgbtn_retry"
            android:layout_width="61.0dip"
            android:layout_height="61.0dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="125.0dip"
            android:background="@color/transparent"
            android:src="@drawable/xiaoying_com_export_redo_icon" />
        <TextView
            android:id="@id/txtview_retry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imgbtn_retry"
            android:layout_alignStart="@id/imgbtn_retry"
            android:layout_alignEnd="@id/imgbtn_retry"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:text="@string/xiaoying_str_studio_share_retry_title"
            android:textColor="@color/white"
            android:textSize="15.0sp" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/layout_4k_retry"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/imgview_center_divider"
        android:visibility="invisible">
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@id/btn_4k_export_retry"
            android:layout_width="200.0dip"
            android:layout_height="44.0dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="125.0dip"
            android:background="@drawable/editor_shape_export_retry_btn_rounded_corner"
            android:ellipsize="end"
            android:gravity="center"
            android:lines="1"
            android:text="@string/xiaoying_str_ve_msg_video_or_prj_export_4k_failed_retrybtn"
            android:textColor="@color/black"
            android:textSize="18.0sp"
            app:autoSizeMaxTextSize="18.0dip"
            app:autoSizeMinTextSize="10.0dip"
            app:autoSizeTextType="bjy" />
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="22.0dip"
            android:layout_height="22.0dip"
            android:layout_alignTop="@id/btn_4k_export_retry"
            android:layout_alignEnd="@id/btn_4k_export_retry"
            android:layout_marginTop="-10.0dip"
            android:layout_marginEnd="1.0dip"
            android:background="@drawable/xiaoying_com_export_fail_retry_flag" />
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@id/btn_normail_export_retry"
            android:layout_width="200.0dip"
            android:layout_height="44.0dip"
            android:layout_below="@id/btn_4k_export_retry"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20.0dip"
            android:background="@drawable/editor_shape_export_sw_retry_btn_rounded_corner"
            android:ellipsize="end"
            android:gravity="center"
            android:lines="1"
            android:text="@string/xiaoying_str_studio_share_retry_title"
            android:textColor="@color/white"
            android:textSize="18.0sp"
            app:autoSizeMaxTextSize="18.0dip"
            app:autoSizeMinTextSize="10.0dip"
            app:autoSizeTextType="bjy" />
    </RelativeLayout>
</RelativeLayout>
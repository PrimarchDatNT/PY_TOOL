<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_dcolor_background"
    android:orientation="horizontal">
    <androidx.recyclerview.widget.RecyclerView
        android:id="@id/filter_def_recyclerview"
        android:layout_width="@dimen/px64"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toEndOf="@id/filter_def_recyclerview"
        android:scrollbarThumbHorizontal="@color/app_dcolor_indicator_2">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@color/app_dcolor_background"
            android:orientation="horizontal">
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_brightness"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="brightness"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_brightness"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_brightness"
                app:fsb_type="avi" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_contrast"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="contrast"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_contrast"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_contrast"
                app:fsb_type="avi" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_sharpen"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="sharpen"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_sharpen"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_sharpen"
                app:fsb_type="avi" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_temperature"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="temperature"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_temperature"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_temperature"
                app:fsb_type="avi" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_vignette"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="vignette"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_vignette"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_vignette"
                app:fsb_type="avi" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_hue"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="hue"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_hue"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_hue"
                app:fsb_type="avi" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_shadow"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="shadow"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_shadow"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_shadow"
                app:fsb_type="avi" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_highlight"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="highlight"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_highlight"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_highlight"
                app:fsb_type="avi" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_fade"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="fade"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_fade"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_fade"
                app:fsb_type="bl9" />
            <com.videoai.aivpcore.editorx.board.filter.FilterReverseSeekBarView
                android:id="@id/filter_param_saturation"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:tag="saturation"
                app:fsb_max_progress="100"
                app:fsb_top_img="@drawable/editorx_filter_param_img_saturation"
                app:fsb_top_name_text="@string/xiaoying_str_ve_video_param_saturation"
                app:fsb_type="avi" />
        </LinearLayout>
    </HorizontalScrollView>
    <View
        android:layout_width="@dimen/px1"
        android:layout_height="match_parent"
        android:layout_toEndOf="@id/filter_def_recyclerview"
        android:background="@color/app_dcolor_bg_1" />
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:rotateView="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/main_view"
        android:layout_width="75.0dip"
        android:layout_height="90.0dip">
        <com.videoai.aivpcore.ui.view.RotateImageView
            android:id="@+id/wheel_img_rect_f"
            android:layout_width="2.0dip"
            android:layout_height="50.0dip"
            android:layout_centerVertical="true"
            android:layout_marginStart="10.0dip"
            android:layout_marginLeft="10.0dip"
            android:background="@drawable/v5_xiaoying_cam_effect_focus_shape"
            android:scaleType="fitXY" />
        <RelativeLayout
            android:layout_width="63.0dip"
            android:layout_height="90.0dip"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true">
            <RelativeLayout
                android:layout_width="53.0dip"
                android:layout_height="90.0dip"
                android:layout_centerInParent="true"
                android:layout_margin="5.0dip">
                <com.videoai.aivpcore.ui.view.RotateImageView
                    android:id="@+id/wheel_more"
                    android:layout_width="53.0dip"
                    android:layout_height="68.0dip"
                    android:layout_centerInParent="true"
                    android:scaleType="fitXY"
                    android:src="@drawable/v4_xiaoying_cam_filter_download"
                    rotateView:rotation="270" />
                <com.videoai.aivpcore.ui.view.RotateImageView
                    android:id="@+id/wheel_img_content"
                    android:layout_width="38.0dip"
                    android:layout_height="68.0dip"
                    android:layout_centerInParent="true"
                    android:scaleType="fitXY" />
                <com.videoai.aivpcore.ui.view.RotateImageView
                    android:id="@+id/img_new_flag"
                    android:layout_width="14.0dip"
                    android:layout_height="30.0dip"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/xiaoying_ve_template_new_flag_hor"
                    android:visibility="visible" />
                <com.videoai.aivpcore.ui.view.RotateImageView
                    android:id="@+id/img_mission_bg_mark"
                    android:layout_width="38.0dip"
                    android:layout_height="68.0dip"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/xiaoying_com_mission_lock_mark_bg"
                    android:visibility="invisible"
                    rotateView:rotation="270" />
                <com.videoai.aivpcore.ui.view.RotateImageView
                    android:id="@+id/img_mission_flag"
                    android:layout_width="25.0dip"
                    android:layout_height="25.0dip"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/xiaoying_com_mission_lock_weibo_mark"
                    android:visibility="invisible"
                    rotateView:rotation="270" />
                <com.videoai.aivpcore.ui.view.RotateImageView
                    android:id="@+id/img_download_flag"
                    android:layout_width="25.0dip"
                    android:layout_height="25.0dip"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/xiaoying_com_mission_download_mark"
                    android:visibility="invisible"
                    rotateView:rotation="270" />
                <com.videoai.aivpcore.ui.view.RotateProgressBar
                    android:id="@+id/download_progress"
                    style="@style/xiaoying_template_download_progressBar"
                    android:layout_width="5.0dip"
                    android:layout_height="68.0dip"
                    android:layout_centerInParent="true"
                    android:progress="0"
                    android:visibility="invisible" />
            </RelativeLayout>
        </RelativeLayout>
        <com.videoai.aivpcore.ui.view.RotateTextView
            android:id="@+id/wheel_txt"
            android:layout_width="15.0dip"
            android:layout_height="match_parent"
            android:textColor="@color/white"
            android:textSize="@dimen/effect_panel_desc_text_size"
            android:visibility="invisible"
            rotateView:isShadowOn="false"
            rotateView:rotation="270" />
    </RelativeLayout>
</RelativeLayout>
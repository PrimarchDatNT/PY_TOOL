<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/cover_cut_root_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <com.videoai.aivpcore.editorx.board.clip.cover.CropImageView
        android:id="@id/cover_crop_image_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:civ_drop_text="@string/xiaoying_str_select_cover_swipe_desc"
        app:civ_drop_text_color="@color/white"
        app:civ_drop_text_left_right_space="24.0dip"
        app:civ_drop_text_size="10.0sp"
        app:civ_stroke_color="@color/white"
        app:civ_stroke_top_bottom_min_space="46.0dip"
        app:civ_stroke_width="2.0dip"
        app:civ_trans_color="@color/app_dcolor_background" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@id/cover_replace_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/px12"
        android:background="@drawable/btn_dcolor_ripple"
        android:drawableStart="@drawable/editorx_cover_replace_btn"
        android:drawablePadding="@dimen/px8"
        android:paddingStart="@dimen/px12"
        android:paddingTop="@dimen/px4"
        android:paddingEnd="@dimen/px12"
        android:paddingBottom="@dimen/px4"
        android:text="@string/xiaoying_str_edit_reselect_title"
        android:textColor="@color/white"
        android:textSize="@dimen/sp12"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="@id/cover_crop_image_view"
        app:layout_constraintStart_toStartOf="@id/cover_crop_image_view" />
</androidx.constraintlayout.widget.ConstraintLayout>
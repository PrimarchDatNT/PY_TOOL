<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:tag="layout/app_include_creation_editor_main_item_0">
    <ImageView
        android:id="@id/imgBg"
        android:layout_width="match_parent"
        android:layout_height="64.0dip"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <ImageView
        android:id="@id/imgShadow"
        android:layout_width="match_parent"
        android:layout_height="32.0dip"
        android:layout_marginTop="60.0dip"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.mobile.component.imageview.DynamicLoadingImageView
        android:id="@id/imgIcon"
        android:layout_width="24.0dip"
        android:layout_height="24.0dip"
        app:actualImageScaleType="yv"
        app:layout_constraintBottom_toBottomOf="@id/imgBg"
        app:layout_constraintEnd_toStartOf="@id/tvName"
        app:layout_constraintHorizontal_chainStyle="apo"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="@id/imgBg" />
    <TextView
        android:id="@id/tvName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8.0dip"
        android:layout_marginLeft="8.0dip"
        android:textColor="@android:color/white"
        android:textSize="17.0sp"
        app:layout_constraintBottom_toBottomOf="@id/imgIcon"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toEndOf="@id/imgIcon"
        app:layout_constraintTop_toTopOf="@id/imgIcon" />
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/open_rl_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:id="@id/open_header_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@id/cancel"
            android:layout_width="wrap_content"
            android:layout_height="44.0dip"
            android:gravity="center"
            android:paddingStart="19.0dip"
            android:paddingLeft="19.0dip"
            android:paddingEnd="19.0dip"
            android:paddingRight="19.0dip"
            android:scaleType="centerInside"
            android:src="@drawable/selector_web_authorize_titlebar_back" />
    </RelativeLayout>
    <FrameLayout
        android:id="@id/open_loading_group"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/open_header_view"
        android:visibility="gone" />
</RelativeLayout>
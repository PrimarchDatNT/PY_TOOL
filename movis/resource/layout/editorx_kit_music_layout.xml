<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="190.0dip"
    android:background="@color/color_141414"
    android:paddingStart="15.0dip"
    android:paddingEnd="15.0dip">
    <LinearLayout
        android:id="@+id/layout_bgm_no_add"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:layout_marginTop="24.0dip"
        android:background="@drawable/editorx_shape_audio_bgm_add_layout"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parrent"
        app:layout_constraintRight_toRightOf="parrent"
        app:layout_constraintTop_toTopOf="parrent">
        <ImageView
            android:layout_width="16.0dip"
            android:layout_height="16.0dip"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="14.0dip"
            android:scaleType="centerCrop"
            android:src="@drawable/editorx_audio_bg_item_add_icon" />
        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:orientation="horizontal">
            <com.videoai.aivpcore.xyui.MarqueeTextView
                android:id="@+id/bgm_add"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="6.0dip"
                android:ellipsize="marquee"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:singleLine="true"
                android:text="@string/xiaoying_str_edit_add_bgm_music_text"
                android:textColor="@color/color_e6e6e6"
                android:textSize="14.0sp" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/layout_bgm_item"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:layout_marginTop="24.0dip"
        android:background="@drawable/editorx_shape_kit_text_adapter"
        android:orientation="horizontal"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="parrent"
        app:layout_constraintRight_toRightOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/layout_bgm_no_add">
        <ImageView
            android:layout_width="16.0dip"
            android:layout_height="16.0dip"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="14.0dip"
            android:scaleType="centerCrop"
            android:src="@drawable/editorx_audio_bg_item_icon" />
        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:orientation="horizontal">
            <com.videoai.aivpcore.xyui.MarqueeTextView
                android:id="@+id/bgm_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="6.0dip"
                android:ellipsize="marquee"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:marqueeRepeatLimit="marquee_forever"
                android:scrollHorizontally="true"
                android:singleLine="true"
                android:textColor="@color/color_808080"
                android:textSize="14.0sp" />
        </LinearLayout>
        <View
            android:layout_width="1.0dip"
            android:layout_height="16.0dip"
            android:layout_gravity="center_vertical"
            android:background="@color/color_363636" />
        <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
            android:id="@+id/bgm_item_replace"
            android:layout_width="60.0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            app:sitv_bottom_text="@string/xiaoying_str_edit_replace_text"
            app:sitv_top_drawable="@drawable/editorx_audio_bgm_replace_icon" />
        <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
            android:id="@+id/bgm_volume"
            android:layout_width="60.0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="6.0dip"
            app:sitv_bottom_text="@string/xiaoying_str_edit_audio_vloume_text"
            app:sitv_top_text="100"
            app:sitv_top_text_color="@drawable/editorx_selector_sitv_text_color"
            app:sitv_top_type="bac" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/layout_ori_item"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:layout_marginTop="20.0dip"
        android:background="@drawable/editorx_shape_kit_text_adapter"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parrent"
        app:layout_constraintRight_toRightOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/layout_bgm_item">
        <ImageView
            android:layout_width="16.0dip"
            android:layout_height="16.0dip"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="14.0dip"
            android:scaleType="centerCrop"
            android:src="@drawable/editorx_audio_ori_item_icon" />
        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/ori_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="6.0dip"
                android:text="@string/xiaoying_str_magic_sound_original"
                android:textColor="@color/color_808080"
                android:textSize="14.0sp" />
        </LinearLayout>
        <View
            android:layout_width="1.0dip"
            android:layout_height="16.0dip"
            android:layout_gravity="center_vertical"
            android:background="@color/color_363636" />
        <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
            android:id="@+id/ori_item_mute"
            android:layout_width="60.0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            app:sitv_bottom_text="@string/xiaoying_str_ve_basic_mute_title"
            app:sitv_top_drawable="@drawable/editorx_selector_audio_mute" />
        <com.videoai.aivpcore.editorx.board.effect.ui.SimpleIconTextView
            android:id="@+id/ori_volume"
            android:layout_width="60.0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="6.0dip"
            app:sitv_bottom_text="@string/xiaoying_str_edit_audio_vloume_text"
            app:sitv_top_text="100"
            app:sitv_top_text_color="@drawable/editorx_selector_sitv_text_color"
            app:sitv_top_type="bac" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
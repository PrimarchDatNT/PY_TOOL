<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@id/main_view"
        android:layout_width="76.0dip"
        android:layout_height="75.0dip">
        <ImageView
            android:id="@id/wheel_img_rect_f"
            android:layout_width="60.0dip"
            android:layout_height="2.0dip"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="4.0dip"
            android:background="@drawable/v5_xiaoying_cam_effect_focus_shape"
            android:scaleType="fitXY"
            android:visibility="visible" />
        <RelativeLayout
            android:layout_width="63.0dip"
            android:layout_height="63.0dip"
            android:layout_centerHorizontal="true">
            <RelativeLayout
                android:layout_width="53.0dip"
                android:layout_height="53.0dip"
                android:layout_centerInParent="true"
                android:layout_margin="5.0dip">
                <com.videoai.aivpcore.ui.view.SafeDrawImageView
                    android:id="@id/wheel_more"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:scaleType="fitXY"
                    android:src="@drawable/v4_xiaoying_cam_filter_download" />
                <com.videoai.aivpcore.ui.view.SafeDrawImageView
                    android:id="@id/wheel_img_content"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY" />
                <ImageView
                    android:id="@id/img_new_flag"
                    android:layout_width="30.0dip"
                    android:layout_height="14.0dip"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/xiaoying_ve_template_new_flag"
                    android:visibility="invisible" />
                <ImageView
                    android:id="@id/img_mission_bg_mark"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/xiaoying_com_mission_lock_mark_bg"
                    android:visibility="invisible" />
                <ImageView
                    android:id="@id/img_mission_flag"
                    android:layout_width="25.0dip"
                    android:layout_height="25.0dip"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/xiaoying_com_mission_lock_weibo_mark"
                    android:visibility="invisible" />
                <ImageView
                    android:id="@id/img_download_flag"
                    android:layout_width="25.0dip"
                    android:layout_height="25.0dip"
                    android:layout_centerInParent="true"
                    android:background="@color/transparent"
                    android:scaleType="fitXY"
                    android:src="@drawable/xiaoying_com_mission_download_mark"
                    android:visibility="invisible" />
                <ProgressBar
                    android:id="@id/download_progress"
                    style="@style/xiaoying_template_download_progressBar"
                    android:layout_width="48.0dip"
                    android:layout_height="5.0dip"
                    android:layout_centerInParent="true"
                    android:progress="0"
                    android:visibility="invisible" />
            </RelativeLayout>
        </RelativeLayout>
        <TextView
            android:id="@id/wheel_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:singleLine="true"
            android:textColor="@color/white"
            android:textSize="@dimen/effect_panel_desc_text_size"
            android:visibility="invisible" />
    </RelativeLayout>
</RelativeLayout>
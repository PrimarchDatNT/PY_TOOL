<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15.0dip"
        android:textColor="@color/color_525252"
        android:textSize="12.0sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.aivpcore.xyui.RoundedTextView
        android:id="@+id/btnLeft"
        android:layout_width="wrap_content"
        android:layout_height="24.0dip"
        android:layout_marginStart="16.0dip"
        android:gravity="center"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip"
        android:textColor="@color/editorx_sel_ratio_text"
        android:textSize="10.0sp"
        android:textStyle="bold"
        app:VS_CornerRadius="12.0dip"
        app:VS_SolidColor="@color/editorx_sel_212121_transparent"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintStart_toEndOf="@id/tvTitle"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.aivpcore.xyui.RoundedTextView
        android:id="@+id/btnCenter"
        android:layout_width="wrap_content"
        android:layout_height="24.0dip"
        android:layout_marginStart="4.0dip"
        android:gravity="center"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip"
        android:textColor="@color/editorx_sel_ratio_text"
        android:textSize="10.0sp"
        android:textStyle="bold"
        app:VS_CornerRadius="12.0dip"
        app:VS_SolidColor="@color/editorx_sel_212121_transparent"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintStart_toEndOf="@id/btnLeft"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.aivpcore.xyui.RoundedTextView
        android:id="@+id/btnRight"
        android:layout_width="wrap_content"
        android:layout_height="24.0dip"
        android:layout_marginStart="4.0dip"
        android:gravity="center"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip"
        android:textColor="@color/editorx_sel_ratio_text"
        android:textSize="10.0sp"
        android:textStyle="bold"
        app:VS_CornerRadius="12.0dip"
        app:VS_SolidColor="@color/editorx_sel_212121_transparent"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintStart_toEndOf="@id/btnCenter"
        app:layout_constraintTop_toTopOf="parrent" />
</androidx.constraintlayout.widget.ConstraintLayout>
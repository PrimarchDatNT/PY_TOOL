<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_original_root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/transparent"
    android:orientation="vertical">
    <View
        android:id="@+id/volume_bg"
        android:layout_width="match_parent"
        android:layout_height="130.0dip"
        android:layout_marginTop="38.0dip"
        android:background="@drawable/editorx_shape_audio_volume_dialog_bg"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <com.videoai.aivpcore.editorx.widget.seekbar.PopSeekBar
        android:id="@+id/volume_pop_seek_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent"
        app:psb_bg_color="@color/default_background_pb_progress_color"
        app:psb_max_progress="200"
        app:psb_seek_bar_color="@color/default_app_color"
        app:psb_seek_bar_left_padding="14.0dip"
        app:psb_seek_bar_progress_drawable="@drawable/psb_progress_drawable"
        app:psb_seek_bar_right_padding="14.0dip"
        app:psb_seek_bar_thumb="@drawable/psb_selector_seek_bar_thumb"
        app:psb_text_bg_space="24.0dip"
        app:psb_top_text_color="@color/color_333333"
        app:psb_top_text_diam="44.0dip"
        app:psb_top_text_size="@dimen/sp16" />
    <TextView
        android:id="@+id/volume_fade_in_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/px16"
        android:layout_marginBottom="@dimen/px12"
        android:ellipsize="end"
        android:gravity="center_horizontal"
        android:maxWidth="@dimen/px64"
        android:maxLines="2"
        android:text="@string/xiaoying_str_edit_audio_volume_fade_in_text"
        android:textColor="@color/white"
        android:textSize="@dimen/sp12"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintStart_toStartOf="parrent" />
    <Switch
        android:id="@+id/volume_fade_in_switch_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/px8"
        android:thumb="@drawable/editorx_selector_switch_thumb"
        android:track="@drawable/editorx_selector_switch_track"
        app:layout_constraintBottom_toBottomOf="@id/volume_fade_in_text"
        app:layout_constraintStart_toEndOf="@id/volume_fade_in_text"
        app:layout_constraintTop_toTopOf="@id/volume_fade_in_text" />
    <Switch
        android:id="@+id/volume_fade_out_switch_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/px16"
        android:layout_marginBottom="@dimen/px12"
        android:thumb="@drawable/editorx_selector_switch_thumb"
        android:track="@drawable/editorx_selector_switch_track"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent" />
    <TextView
        android:id="@+id/volume_fade_out_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/px8"
        android:ellipsize="end"
        android:gravity="center_horizontal"
        android:maxWidth="@dimen/px64"
        android:maxLines="2"
        android:text="@string/xiaoying_str_edit_audio_volume_fade_out_text"
        android:textColor="@color/white"
        android:textSize="@dimen/sp12"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/volume_fade_out_switch_text"
        app:layout_constraintEnd_toStartOf="@id/volume_fade_out_switch_text"
        app:layout_constraintTop_toTopOf="@id/volume_fade_out_switch_text" />
    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50.0dip"
        android:text="@string/xiaoying_str_edit_audio_volume_change_text"
        android:textColor="@color/white"
        android:textSize="@dimen/sp12"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
</androidx.constraintlayout.widget.ConstraintLayout>
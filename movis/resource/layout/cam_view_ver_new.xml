<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.videoai.aivpcore.camera.ui.view.indicator.TopIndicatorNew
        android:id="@id/cam_layout_title_new"
        android:layout_width="match_parent"
        android:layout_height="@dimen/v4_xiaoying_cam_top_bar_height" />
    <com.videoai.aivpcore.camera.ui.view.BackDeleteProgressBar
        android:id="@id/back_delete_progressbar"
        android:layout_width="match_parent"
        android:layout_height="8.0dip"
        android:visibility="gone" />
    <RelativeLayout
        android:id="@id/effect_tip_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="134.0dip"
        android:visibility="gone">
        <TextView
            android:id="@id/txt_effect_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:shadowColor="#80000000"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="6.0"
            android:textColor="@color/white"
            android:textSize="25.0sp" />
    </RelativeLayout>
    <com.videoai.aivpcore.camera.ui.shutter.CamShutterLayout
        android:id="@id/shutter_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/transparent" />
    <LinearLayout
        android:id="@id/zoom_tip_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@id/shutter_layout"
        android:layout_marginBottom="30.0dip"
        android:orientation="horizontal"
        android:visibility="gone">
        <ImageView
            android:id="@id/zoom_left_btn"
            android:layout_width="30.0dip"
            android:layout_height="25.0dip"
            android:layout_gravity="end|center"
            android:layout_marginStart="5.0dip"
            android:layout_marginLeft="5.0dip"
            android:paddingStart="10.0dip"
            android:paddingLeft="10.0dip"
            android:paddingEnd="0.0dip"
            android:paddingRight="0.0dip"
            android:scaleType="centerInside"
            android:src="@drawable/v6_xiaoying_cam_zoom_left_bg" />
        <SeekBar
            android:id="@id/zoom_progress"
            style="@style/xiaoying_zoom_progressBar"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1.0"
            android:thumbOffset="5.0dip" />
        <ImageView
            android:id="@id/zoom_right_btn"
            android:layout_width="30.0dip"
            android:layout_height="25.0dip"
            android:layout_gravity="start|center"
            android:layout_marginEnd="5.0dip"
            android:layout_marginRight="5.0dip"
            android:paddingStart="0.0dip"
            android:paddingLeft="0.0dip"
            android:paddingEnd="10.0dip"
            android:paddingRight="10.0dip"
            android:scaleType="centerInside"
            android:src="@drawable/v6_xiaoying_cam_zoom_right_bg" />
    </LinearLayout>
    <com.videoai.aivpcore.camera.ui.view.MusicControlView
        android:id="@id/xiaoying_cam_mode_music_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="155.0dip"
        android:visibility="invisible" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/cam_layout_title_new"
        android:layout_marginTop="-12.0dip"
        android:orientation="vertical">
        <com.videoai.aivpcore.camera.ui.view.BackDeleteTextButton
            android:id="@id/cam_btn_delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:visibility="gone" />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip">
            <include
                android:id="@id/iap_vip_tip"
                layout="@layout/iap_toast_vip_tip_in_camera"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone" />
        </FrameLayout>
    </LinearLayout>
    <TextView
        android:id="@id/cam_hint_for_lan"
        style="@style/cam_hint_text_style"
        android:layout_width="wrap_content"
        android:layout_height="24.0dip"
        android:layout_below="@id/cam_layout_title_new"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40.0dip"
        android:text="@string/xiaoying_str_cam_help_snap_tip_lan"
        android:visibility="invisible" />
    <TextView
        android:id="@id/cam_hint_for_por_left"
        style="@style/cam_hint_text_style"
        android:layout_width="wrap_content"
        android:layout_height="24.0dip"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:rotation="-90.0"
        android:text="@string/xiaoying_str_cam_help_snap_tip_por"
        android:visibility="invisible" />
    <TextView
        android:id="@id/cam_hint_for_por_right"
        style="@style/cam_hint_text_style"
        android:layout_width="wrap_content"
        android:layout_height="24.0dip"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:rotation="90.0"
        android:text="@string/xiaoying_str_cam_help_snap_tip_por"
        android:visibility="invisible" />
    <RelativeLayout
        android:id="@id/layout_2lev"
        android:layout_width="match_parent"
        android:layout_height="136.0dip"
        android:layout_alignBottom="@id/shutter_layout"
        android:background="#80000000"
        android:clickable="true"
        android:visibility="gone">
        <com.videoai.aivpcore.ui.view.HotFixRecyclerView
            android:id="@id/effect_listview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true" />
    </RelativeLayout>
    <com.videoai.aivpcore.camera.ui.facial.CameraFacialView
        android:id="@id/pasterpanel"
        android:layout_width="match_parent"
        android:layout_height="215.0dip"
        android:layout_alignParentBottom="true"
        android:visibility="gone" />
    <com.videoai.aivpcore.camera.ui.view.BeautyLevelBar
        android:id="@id/xiaoying_cam_fb_value_bar_por"
        android:layout_width="match_parent"
        android:layout_height="120.0dip"
        android:layout_above="@id/shutter_layout"
        android:visibility="gone" />
    <TextView
        android:id="@id/layout_aelock_tip"
        android:layout_width="wrap_content"
        android:layout_height="24.0dip"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="50.0dip"
        android:layout_marginEnd="15.0dip"
        android:layout_marginRight="15.0dip"
        android:background="@drawable/xiaoying_cam_common_tip_bg"
        android:gravity="center"
        android:maxLines="1"
        android:minWidth="80.0dip"
        android:paddingStart="8.0dip"
        android:paddingLeft="8.0dip"
        android:paddingEnd="8.0dip"
        android:paddingRight="8.0dip"
        android:text="@string/xiaoying_str_cam_aelock"
        android:textColor="@color/white"
        android:textSize="12.0sp"
        android:visibility="invisible" />
    <RelativeLayout
        android:id="@id/layout_swipe_filter_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="160.0dip"
        android:visibility="gone">
        <ImageView
            android:id="@id/tool_filter_effect_tip_hand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:src="@drawable/xiaoying_cam_tool_filter_effect_tip_hand" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24.0dip"
            android:layout_below="@id/tool_filter_effect_tip_hand"
            android:layout_centerHorizontal="true"
            android:background="@drawable/xiaoying_cam_common_tip_bg"
            android:gravity="center"
            android:paddingStart="10.0dip"
            android:paddingLeft="10.0dip"
            android:paddingEnd="10.0dip"
            android:paddingRight="10.0dip"
            android:text="@string/xiaoying_str_help_cam_swipe_to_change_filter"
            android:textColor="@color/white"
            android:textSize="12.0sp" />
    </RelativeLayout>
</RelativeLayout>
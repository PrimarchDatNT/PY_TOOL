<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/px68"
    android:layout_height="@dimen/px80"
    android:tag="layout/templatex_view_list_item_xxs_0">
    <com.google.android.material.imageview.ShapeableImageView
        android:id="@id/imgCover"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px60"
        android:layout_gravity="center_horizontal"
        android:layout_margin="@dimen/px1"
        android:background="@color/app_dcolor_bg_1"
        android:scaleType="fitXY"
        android:tag="binding_1"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent"
        app:shapeAppearanceOverlay="@style/RoundedImageItem" />
    <View
        android:layout_width="@dimen/px68"
        android:layout_height="@dimen/px80"
        android:background="@drawable/templatex_shape_item_bg"
        android:tag="binding_2" />
    <View
        android:id="@id/imgFocus"
        android:layout_width="@dimen/px68"
        android:layout_height="@dimen/px80"
        android:background="@drawable/templatex_shape_item_focus"
        android:tag="binding_3" />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@id/tvTitle"
        android:layout_width="@dimen/px68"
        android:layout_height="@dimen/px20"
        android:layout_gravity="bottom"
        android:gravity="center"
        android:tag="binding_4"
        android:textColor="@color/text_selector_tab"
        android:textSize="@dimen/sp10"
        app:layout_constraintEnd_toEndOf="@id/imgCover"
        app:layout_constraintStart_toStartOf="@id/imgCover"
        app:layout_constraintTop_toBottomOf="@id/imgCover" />
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@id/imgNew"
        android:layout_width="26.0dip"
        android:layout_height="12.0dip"
        android:layout_marginTop="8.0dip"
        android:layout_marginEnd="8.0dip"
        android:src="@drawable/templatex_icon_new"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/imgCover"
        app:layout_constraintTop_toTopOf="@id/imgCover" />
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@id/imgVip"
        android:layout_width="wrap_content"
        android:layout_height="14.0dip"
        android:layout_marginTop="4.0dip"
        android:layout_marginEnd="4.0dip"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="@id/imgCover"
        app:layout_constraintTop_toTopOf="@id/imgCover" />
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@id/imgLock"
        android:layout_width="24.0dip"
        android:layout_height="24.0dip"
        android:layout_marginTop="8.0dip"
        android:layout_marginEnd="8.0dip"
        android:src="@drawable/templatex_icon_lock"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/imgCover"
        app:layout_constraintTop_toTopOf="@id/imgCover" />
</FrameLayout>
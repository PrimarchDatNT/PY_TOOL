<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/px80"
    android:layout_height="@dimen/px80"
    android:background="@drawable/editorx_shape_effect_item_bg_unchoose">
    <RelativeLayout
        android:id="@id/item_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/iv_download"
            android:layout_width="@dimen/px16"
            android:layout_height="@dimen/px16"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_margin="@dimen/px2"
            android:src="@drawable/editorx_template_download_icon" />
        <com.videoai.mobile.component.imageview.DynamicLoadingImageView
            android:id="@id/div_roll_thumb"
            android:layout_width="@dimen/px60"
            android:layout_height="@dimen/px60"
            android:layout_centerInParent="true"
            android:background="@color/transparent"
            app:roundedCornerRadius="2.0dip" />
        <com.videoai.aivpcore.editorx.widget.magic.RoundProgressView
            android:id="@id/progress_download"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            app:backgroundColor="#99020202"
            app:circleCorner="0.0dip"
            app:circleProgress="0"
            app:circleRadius="50.0dip"
            app:startAngle="270" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/layoutMore"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/ivMore"
            android:layout_width="24.0dip"
            android:layout_height="24.0dip"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="24.0dip"
            android:src="@drawable/editorx_effect_subtitle_more" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ivMore"
            android:layout_centerHorizontal="true"
            android:text="@string/xiaoying_str_studio_sns_more"
            android:textColor="@color/color_e6e6e6"
            android:textSize="14.0sp" />
    </RelativeLayout>
</RelativeLayout>
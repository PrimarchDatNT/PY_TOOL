<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/relativelayout_storyboard_panel"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_262626">
    <RelativeLayout
        android:id="@id/layout_preview_bg_wrapper"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@id/xiaoying_ve_pip_bottom_hide_view"
        android:layout_below="@id/btns_layout"
        android:background="@color/transparent">
        <RelativeLayout
            android:id="@id/layout_preview_background"
            android:layout_width="match_parent"
            android:layout_height="320.0dip"
            android:layout_centerInParent="true"
            android:background="@color/transparent"
            android:visibility="visible">
            <RelativeLayout
                android:id="@id/preview_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:background="@color/transparent">
                <SurfaceView
                    android:id="@id/previewview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@id/preview_layout_fake"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignStart="@id/preview_layout"
                android:layout_alignLeft="@id/preview_layout"
                android:layout_alignTop="@id/preview_layout"
                android:layout_alignEnd="@id/preview_layout"
                android:layout_alignRight="@id/preview_layout"
                android:layout_alignBottom="@id/preview_layout"
                android:background="@color/transparent">
                <com.videoai.aivpcore.editor.pip.HighLightView
                    android:id="@id/xiaoying_pip_highlightview_videoarea"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/transparent"
                    android:visibility="gone" />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/btns_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/v4_xiaoying_com_top_panel_height"
        android:layout_alignParentTop="true"
        android:visibility="visible">
        <TextView
            android:id="@id/tv_main_panel_mode_title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/panel_top_height"
            android:layout_centerVertical="true"
            android:layout_marginStart="32.0dip"
            android:layout_marginLeft="32.0dip"
            android:layout_toEndOf="@id/xiaoying_com_btn_left"
            android:layout_toRightOf="@id/xiaoying_com_btn_left"
            android:gravity="center"
            android:text="@string/xiaoying_str_com_home_edit_pip"
            android:textColor="@color/white"
            android:textSize="@dimen/v2_com_top_bar_title_text_size" />
        <ImageView
            android:id="@id/btn_import_finish"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="8.0dip"
            android:layout_marginRight="8.0dip"
            android:scaleType="centerInside"
            android:src="@drawable/v4_xiaoying_com_btn_next_selector"
            android:visibility="visible" />
        <ImageButton
            android:id="@id/xiaoying_ve_preview_layout_captrue"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="8.0dip"
            android:layout_marginRight="8.0dip"
            android:background="@color/transparent"
            android:scaleType="centerInside"
            android:src="@drawable/v4_xiaoying_com_btn_ok_selector"
            android:visibility="visible" />
        <ImageView
            android:id="@id/xiaoying_com_btn_left"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="8.0dip"
            android:layout_marginLeft="8.0dip"
            android:scaleType="centerInside"
            android:src="@drawable/v4_xiaoying_com_btn_back_selector" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/btns_layout2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/v4_xiaoying_com_top_panel_height"
        android:layout_alignParentTop="true"
        android:visibility="gone">
        <TextView
            android:id="@id/textview_title"
            android:layout_width="150.0dip"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginStart="16.0dip"
            android:layout_marginLeft="16.0dip"
            android:gravity="center"
            android:singleLine="true"
            android:text="@string/xiaoying_str_ve_pip_adjust_videos"
            android:textColor="@color/white"
            android:textSize="@dimen/v2_com_top_bar_title_text_size" />
        <ImageButton
            android:id="@id/btn_text_ok"
            android:layout_width="32.0dip"
            android:layout_height="32.0dip"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="8.0dip"
            android:layout_marginRight="8.0dip"
            android:background="@drawable/v4_xiaoying_com_btn_ok_selector"
            android:scaleType="fitXY" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/layout_time_show"
        android:layout_width="match_parent"
        android:layout_height="20.0dip"
        android:layout_above="@id/relativelayout_trimgallery_content_area"
        android:layout_marginBottom="5.0dip"
        android:visibility="invisible">
        <TextView
            android:id="@id/txtview_trimed_duration"
            android:layout_width="50.0dip"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:background="@drawable/xiaoying_ve_simple_trim_duration_bg"
            android:gravity="center"
            android:textColor="@color/color_999999"
            android:textSize="@dimen/v2_preview_time_textsize" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/relativelayout_trimgallery_content_area"
        android:layout_width="match_parent"
        android:layout_height="101.0dip"
        android:layout_above="@id/xiaoying_ve_pip_bottom_view"
        android:visibility="gone">
        <include
            android:id="@id/xiaoying_ve_pip_layout_multi_trim"
            layout="@layout/editor_pip_multi_trim_layout" />
        <TextView
            android:id="@id/xiaoying_ve_txtview_pip_trim_tip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/xiaoying_ve_pip_layout_multi_trim"
            android:layout_centerHorizontal="true"
            android:padding="2.0dip"
            android:text="@string/xiaoying_str_ve_pip_operation_tip"
            android:textColor="@color/color_999999"
            android:textSize="12.0sp"
            android:visibility="gone" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/xiaoying_ve_pip_bottom_hide_view"
        android:layout_width="match_parent"
        android:layout_height="101.0dip"
        android:layout_above="@id/xiaoying_ve_pip_bottom_view"
        android:visibility="visible">
        <RelativeLayout
            android:id="@id/relativelayout_seekbar"
            android:layout_width="match_parent"
            android:layout_height="40.0dip"
            android:layoutDirection="ltr">
            <TextView
                android:id="@id/txtview_cur_time"
                android:layout_width="50.0dip"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="2.0dip"
                android:layout_marginRight="2.0dip"
                android:gravity="center"
                android:singleLine="true"
                android:text=""
                android:textColor="@color/color_5f6366"
                android:textSize="@dimen/v2_preview_time_textsize" />
            <SeekBar
                android:id="@id/seekbar_simple_edit"
                android:layout_width="match_parent"
                android:layout_height="18.0dip"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@id/txtview_duration"
                android:layout_toLeftOf="@id/txtview_duration"
                android:layout_toEndOf="@id/txtview_cur_time"
                android:layout_toRightOf="@id/txtview_cur_time"
                android:focusable="true"
                android:max="100"
                android:maxHeight="2.0dip"
                android:paddingStart="6.5999756dip"
                android:paddingLeft="6.5999756dip"
                android:paddingEnd="6.5999756dip"
                android:paddingRight="6.5999756dip"
                android:progress="50"
                android:progressDrawable="@drawable/v4_xiaoying_ve_preview_seekbar_progress_bg_drawable"
                android:thumb="@drawable/v4_xiaoying_ve_preview_seekbar_slider"
                android:thumbOffset="6.5999756dip"
                android:visibility="visible" />
            <TextView
                android:id="@id/txtview_duration"
                android:layout_width="50.0dip"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="2.0dip"
                android:layout_marginLeft="2.0dip"
                android:gravity="center"
                android:singleLine="true"
                android:text=""
                android:textColor="@color/color_5f6366"
                android:textSize="@dimen/v2_preview_time_textsize" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@id/relativelayout_tool_list_content"
            android:layout_width="match_parent"
            android:layout_height="60.0dip"
            android:layout_alignParentBottom="true"
            android:gravity="center_vertical">
            <HorizontalScrollView
                android:id="@id/hscrollview_tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:scrollbarStyle="insideOverlay"
                android:scrollbars="none">
                <LinearLayout
                    android:id="@id/linearlayout_tools"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" />
            </HorizontalScrollView>
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@id/xiaoying_ve_pip_bottom_view"
        android:layout_width="match_parent"
        android:layout_height="72.0dip"
        android:layout_alignParentBottom="true"
        android:visibility="visible">
        <RelativeLayout
            android:id="@id/xiaoying_ad_edit_layout"
            android:layout_width="match_parent"
            android:layout_height="72.0dip"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="8.299988dip"
            android:layout_marginLeft="8.299988dip"
            android:layout_marginEnd="8.299988dip"
            android:layout_marginRight="8.299988dip">
            <ImageButton
                android:id="@id/xiaoying_ve_pip_btn_exchagne"
                android:layout_width="50.0dip"
                android:layout_height="50.0dip"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="10.0dip"
                android:layout_marginLeft="10.0dip"
                android:background="@color/transparent"
                android:scaleType="fitXY"
                android:src="@drawable/v4_xiaoying_ve_pip_edit_exchange_btn_selector"
                android:visibility="visible" />
            <ImageButton
                android:id="@id/btn_play"
                android:layout_width="58.0dip"
                android:layout_height="58.0dip"
                android:layout_centerInParent="true"
                android:background="@color/transparent"
                android:scaleType="fitXY"
                android:src="@drawable/v4_xiaoying_ve_pip_edit_play_btn_selector"
                android:visibility="visible" />
            <ImageButton
                android:id="@id/btn_pause"
                android:layout_width="58.0dip"
                android:layout_height="58.0dip"
                android:layout_centerInParent="true"
                android:background="@color/transparent"
                android:scaleType="fitXY"
                android:src="@drawable/v4_xiaoying_ve_pip_edit_pause_btn_selector"
                android:visibility="invisible" />
            <ImageButton
                android:id="@id/xiaoying_ve_pip_btn_trim"
                android:layout_width="50.0dip"
                android:layout_height="50.0dip"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="10.0dip"
                android:layout_marginRight="10.0dip"
                android:background="@color/transparent"
                android:scaleType="fitXY"
                android:src="@drawable/v4_xiaoying_ve_pip_edit_trim_btn_selector"
                android:visibility="visible" />
            <ImageButton
                android:id="@id/xiaoying_btn_hide"
                android:layout_width="50.0dip"
                android:layout_height="50.0dip"
                android:layout_alignStart="@id/xiaoying_ve_pip_btn_trim"
                android:layout_alignLeft="@id/xiaoying_ve_pip_btn_trim"
                android:layout_alignTop="@id/xiaoying_ve_pip_btn_trim"
                android:layout_alignEnd="@id/xiaoying_ve_pip_btn_trim"
                android:layout_alignRight="@id/xiaoying_ve_pip_btn_trim"
                android:layout_alignBottom="@id/xiaoying_ve_pip_btn_trim"
                android:background="@color/transparent" />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.videoai.aivpcore.editor.export.beaut.VideoDescEditorView
        android:id="@+id/layoutDesc"
        android:layout_width="0.0dip"
        android:layout_height="50.0dip"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toTopOf="parrent" />
    <View
        android:id="@+id/viewDivider"
        android:layout_width="0.0dip"
        android:layout_height="0.5dip"
        android:background="@color/color_eeeeee"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/layoutDesc" />
    <com.videoai.aivpcore.editor.export.beaut.ExportTipsView
        android:id="@+id/layoutTips"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/viewDivider" />
    <com.videoai.aivpcore.editor.share.ExportShareView
        android:id="@+id/layoutShare"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parrent"
        app:layout_constraintEnd_toEndOf="parrent"
        app:layout_constraintStart_toStartOf="parrent"
        app:layout_constraintTop_toBottomOf="@id/viewDivider" />
</androidx.constraintlayout.widget.ConstraintLayout>
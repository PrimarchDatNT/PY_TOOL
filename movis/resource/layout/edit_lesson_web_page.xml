<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="44.0dip"
        android:background="@color/color_141414">
        <ImageButton
            android:id="@+id/editor_back_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_centerVertical="true"
            android:background="@null"
            android:contentDescription="@null"
            android:paddingStart="11.0dip"
            android:paddingLeft="11.0dip"
            android:paddingTop="10.0dip"
            android:paddingEnd="11.0dip"
            android:paddingRight="11.0dip"
            android:paddingBottom="10.0dip"
            android:src="@drawable/v4_xiaoying_cam_btn_cam_cancel" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/xiaoying_str_editor_edit_help"
                android:textColor="@color/white"
                android:textSize="18.0sp" />
        </RelativeLayout>
    </RelativeLayout>
    <WebView
        android:id="@+id/webview_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/title" />
</RelativeLayout>
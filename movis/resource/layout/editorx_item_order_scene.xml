<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/px68"
    android:layout_height="@dimen/px68">
    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv_scene"
        android:layout_width="@dimen/px68"
        android:layout_height="@dimen/px68"
        android:scaleType="centerCrop"
        android:src="@drawable/app_logon_2"
        app:shapeAppearanceOverlay="@style/RoundedRectItem" />
    <TextView
        android:id="@+id/tv_scene_num"
        android:layout_width="@dimen/px20"
        android:layout_height="@dimen/px16"
        android:layout_marginStart="@dimen/px6"
        android:layout_marginTop="@dimen/px6"
        android:background="@drawable/editorx_shape_order_scene_num"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/sp10" />
</FrameLayout>
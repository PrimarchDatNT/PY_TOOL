<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/editorx_shape_effect_item_bg_unchoose_noradius">
    <com.videoai.mobile.component.imageview.DynamicLoadingImageView
        android:id="@+id/collage_gif_item_cover"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:background="@color/transparent"
        android:padding="2.0dip" />
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/collage_gif_download_flag"
        android:layout_width="16.0dip"
        android:layout_height="16.0dip"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="5.0dip"
        android:layout_marginBottom="3.0dip"
        android:scaleType="centerCrop"
        android:src="@drawable/editorx_template_download_icon"
        android:visibility="visible" />
    <View
        android:id="@+id/layout_choose"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/editorx_shape_effect_item_bg_noradius_unchoose" />
    <com.videoai.aivpcore.editorx.widget.magic.RoundProgressView
        android:id="@+id/progress_download"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:backgroundColor="#99020202"
        app:circleCorner="0.0dip"
        app:circleProgress="0"
        app:startAngle="270" />
</RelativeLayout>
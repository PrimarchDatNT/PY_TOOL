<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <FrameLayout
        android:id="@id/tt_insert_express_ad_fl"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="32.0dip"
        android:visibility="gone" />
    <ImageView
        android:id="@id/tt_insert_ad_img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32.0dip"
        android:scaleType="fitXY" />
    <ImageView
        android:id="@id/tt_insert_dislike_icon_img"
        android:layout_width="20.0dip"
        android:layout_height="20.0dip"
        android:layout_gravity="end|center|top"
        android:layout_marginTop="10.0dip"
        android:scaleType="fitXY"
        android:src="@drawable/tt_dislike_icon" />
    <ImageView
        android:id="@id/tt_insert_ad_logo"
        android:layout_width="36.0dip"
        android:layout_height="14.0dip"
        android:layout_gravity="end|bottom|center"
        android:layout_marginEnd="10.0dip"
        android:layout_marginRight="10.0dip"
        android:layout_marginBottom="15.0dip"
        android:background="@drawable/tt_ad_logo_new" />
    <TextView
        android:id="@id/tt_insert_ad_text"
        android:layout_width="wrap_content"
        android:layout_height="20.0dip"
        android:layout_gravity="start|bottom|center"
        android:layout_marginStart="10.0dip"
        android:layout_marginLeft="10.0dip"
        android:layout_marginBottom="15.0dip"
        android:background="@drawable/tt_circle_solid_mian"
        android:gravity="center"
        android:text="@string/tt_ad"
        android:textColor="@color/tt_white" />
</FrameLayout>
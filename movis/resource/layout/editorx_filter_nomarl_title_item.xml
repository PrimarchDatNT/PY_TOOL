    android:layout_height="match_parent"
    android:paddingStart="@dimen/px6"
    android:paddingEnd="@dimen/px6">
    <TextView
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/slt_item_tag"
        android:gravity="center"
        android:paddingStart="@dimen/px12"
        android:paddingTop="@dimen/px6"
        android:paddingEnd="@dimen/px12"
        android:paddingBottom="@dimen/px6"
        android:textColor="@color/text_selector_tab" />
    <View
        android:id="@+id/v_select"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:background="@drawable/editorx_shape_template_title" />
</FrameLayout>
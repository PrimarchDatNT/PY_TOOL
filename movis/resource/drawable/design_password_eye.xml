<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@id/bnw" android:drawable="@drawable/design_ic_visibility_off" android:state_checked="true" />
    <item
        android:id="@id/ajd" android:drawable="@drawable/design_ic_visibility" />
    <transition
        android:drawable="@drawable/hg" android:fromId="@id/ajd" android:toId="@id/visible" />
    <transition
        android:drawable="@drawable/hh" android:fromId="@id/bnw" android:toId="@id/masked" />
</animated-selector>
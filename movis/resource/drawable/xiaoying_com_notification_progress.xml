<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient
                android:angle="0.0" android:centerColor="#ff303030" android:endColor="#ff303030" android:startColor="#ff303030" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient
                    android:angle="270.0" android:centerColor="#80c07ab8" android:centerY="0.75" android:endColor="#a0c07ab8" android:startColor="#80c07ab8" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient
                    android:angle="0.0" android:centerColor="#ff32b1e0" android:endColor="#ff32b1e0" android:startColor="#ff32b1e0" />
            </shape>
        </clip>
    </item>
</layer-list>
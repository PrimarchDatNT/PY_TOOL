<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/fcm_notification_detail_dialog_cta_pressed" />
            <corners android:radius="@dimen/fcm_notification_detail_dialog_corner_cta" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/fcm_notification_detail_dialog_cta_normal" />
            <corners android:radius="@dimen/fcm_notification_detail_dialog_corner_cta" />
        </shape>
    </item>
</selector>